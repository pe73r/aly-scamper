		.slick-prev {
			background-color: $c-primary;
			transition: 0.3s opacity;
			&::after { color: $c-background; }
		}
		.slick-dots {
			li {
				border-color: #FFFFFF;
				&.slick-active { background-color: #FFFFFF; }
			}
		}
	}
	&.style-nav-text {
		.slick-next,
		.slick-prev {
			background-color: #FFFFFF;
			transition: 0.3s opacity;
