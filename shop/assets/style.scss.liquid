		}
	}
	/**
	 * Trigger for product image zoom
	 */
	 .zoomImg {
		cursor: -moz-zoom-in;
		cursor: -webkit-zoom-in;
		cursor: url('./magnify.cur'), move;
		background-color: #fff;
		@include transition(unquote('top .3s ease-out, left .3s ease-out'));
	}

	.zoom-container {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		@include transition(unquote('transform .45s ease-out'));
		@include transform(unquote('scale(0)'));

		&.zoomed {
			@include transform(unquote('scale(1)'));
		}

		@media screen and (max-width: ($med - 1)) {
			display: none !important;
		}
	}

	/**
	 * Show selected image flag.
	 */
	&.is-selected-product {
		.product-selected-mark, .product-selected-flag {
			opacity: 1;
			pointer-events: auto;
		}
	}
	/**
	 * To fade out and back in after clicking on Variant Popover.
	 */
	&.fadeOut {
		opacity: 0.3;
		&.is-selected-product {
			opacity: 1;
		}
	}
}

