}
.order-product {
	text-align: left;

	.order-product-thumb {
		@include inlineRel;

		background-size: cover;
		background-position: center;
		background-repeat: no-repeat;
		height: 50px;
		margin-right: 10px;
		width: 50px;
		vertical-align: middle;
	}

	a {
		@include inlineRel;

		width: 60%;
		vertical-align: middle;
	}
}

.order-product-discounts {
	padding-top: 5px;
	p {
	    font-size: 0.9rem;
    	opacity: 0.7;
	}
}


/*
 * Addresses
 */
.saved-addresses {
	margin-top: 50px;

	.address {
		margin: 20px 0;

		a {
			line-height: 2.5;
			text-decoration: underline;
		}
	}
}

/*
 * Pagination
 */
hr {
	@include blockRel;

	border: 0;
	border-top: 1px solid $c-border;
}
.pagination {
	margin: 0 0 20px;
	text-align: center;

	.page {
		margin: 10px 10px;
		opacity: .6;
	}

	.page.current {
		opacity: 1;
	}

	.prev {
		float: left;
	}
	.next {
		float: right;
	}

	.account-home & {
		margin-top: 20px;
	}
}

.pagination__list {
	list-style: none;
	display: flex;
	align-items: center;
	justify-content: center;
	width: 100%;
}

.pagination__item {
	margin: 0 5px;

	a {
		display: flex;
		align-items: center;
		justify-content: center;
		text-align: center;
		width: 28px;
		height: 28px;
		border-radius: 50%;
		border: 1px solid $c-border;
	}
}

.tag-label {
	color: $c-text;
}

.blog-listing {
	.article--featured {
		position: relative;
		margin-bottom: 40px;
		padding-left: 5px;
		padding-right: 5px;

		&::after {
			content: '';
			display: table;
			clear: both;
			height: 0;
			line-height: 0;
		}

		.article-image-container {
			position: relative;
			float: right;
			width: calc(50% - 5px);

			.article-image {
				margin: 0;
				padding-top: 89%;
			}

			.details {
				position: absolute;
				top: 100%;
				left: 0;
				width: 100%;
				margin: 10px 0 0;
				text-align: center;
				white-space: nowrap;
				overflow: hidden;
				text-overflow: ellipsis;
				font-family: $general_font;
				font-weight: $general_font_weight;
				font-style: $general_font_style;
				font-size: $font_bodytext_size * 0.86;

				@media screen and (min-width: $med) {
					top: auto;
					bottom: 0;
					width: auto;
					text-align: left;
					max-width: 100%;
					margin: 0 0 0 -8px;
					transform-origin: left bottom;
					-webkit-transform-origin: left bottom;
					@include transform(unquote('rotate(-90deg)'));
				}
			}

			@media screen and (max-width: ($med - 1)) {
				float: none;
				width: 100%;
				margin-bottom: 50px;

				.article-image {
					padding-top: 66%;
				}
			}
		}

		.article-content {
			overflow: hidden;
			padding-right: 50px;
			opacity: 1;
			@include transition(unquote('opacity .15s ease-out'));

			h2 {
				font-size: $font_base_size * 2.5;
				font-family: $header_font;
				font-weight: $header_weight_bold;
				line-height: 1.33;
				margin: 10px 0;

				@media screen and (max-width: ($med - 1)) {
					font-size: $font_base_size * 2;
				}
			}

			p {
				font-size: $font_bodytext_size * 1.15;
				line-height: 1.78;
				margin: 0 0 24px;

				@media screen and (max-width: ($med - 1)) {
					font-size: $font_bodytext_size * 0.86;
				}
			}

			.tags {
				font-size: $font_bodytext_size;
				font-family: $general_font;
				font-weight: $general_font_weight;
				font-style: $general_font_style;

				margin: 0;

				a {
					margin-right: 40px;
					text-decoration: none;
					opacity: .75;

					&:last-child {
						margin-right: 0;
					}

					&:hover {
						opacity: 1;
					}
				}
			}

			.details {
				font-size: $font_bodytext_size * 0.86;
				opacity: .75;
			}
		}

		&.article--no-image {
			text-align: center;

			.article-content {
				padding-right: 0;
				max-width: 725px;
				margin: 0 auto;
			}
		}

		@media screen and (max-width: ($med - 1)) {
			text-align: center;

			.accent-line {
				margin-left: auto;
				margin-right: auto;
			}

			.article-content {
				padding-right: 0;
			}
		}

		&.is-loading {
			.article-content {
				opacity: 0;
			}
		}
	}

	.article {
		.details {
			text-transform: capitalize;
		}
	}

	.blogModule-posts__post {
		margin-bottom: 30px;
	}
}


.blogModule-posts__post--no-image {
	.carousel-slide-image {
		display: none;
	}
}

.blog-title {
	@include inlineRel;
	width: auto;
}
.blog-rss {
	@include transition(opacity .2s);
	font-size: 1rem;
	line-height: 1;
	opacity: .4;
	position: absolute;
	right: -30px;
	top: 40%;
	&:hover {
		opacity: 1;

		span {
			opacity: 1;
		}
	}

	span {
		@include transition(opacity .2s);
		@include h4;
		opacity: 0;
		line-height: 1.1;
		position: absolute;
		left: 200%;
		min-width: 100px;
		text-align: left;
	}
}

.article-image {
	position: relative;
	padding-top: 66%;
	margin-bottom: 30px;

	background-size: cover;
	background-position: 50% 50%;
	background-repeat: no-repeat;

	a {
		position: absolute;
		top: 0; bottom: 0;
		left: 0; right: 0;
	}
