$spinner-color: $c-text;
$spinner-background: $c-background;
$spinner-diamond-size: 20px;
$spinner-diamond-scale: 0.7;
$spinner-z-offset: 60px;
$spinner-perspective: 200px;
$spinner-duration: 1200ms;
$spinner-step-delay: 120ms;

.spinner {
	position: absolute;
	top: 0; bottom: 0;
	left: 0; right: 0;
	margin: auto;
	@include perspective($spinner-perspective);
	background: $spinner-background;
	z-index: 10;

	.popover-inner & {
		@include transform(unquote('scale(0.5)'));
	}
}

.spinner-diamond {
		position: absolute;
		top: 0; bottom: 0;
		left: 0; right: 0;
		width: $spinner-diamond-size;
		height: $spinner-diamond-size;
		margin: auto;

		@include animation(spinDiamond3d $spinner-duration $spinner-step-delay infinite);

		.no-csstransforms3d & {
				width: $spinner-diamond-size * (2 - $spinner-diamond-scale);
				height: $spinner-diamond-size * (2 - $spinner-diamond-scale);
				@include animation(spinDiamond $spinner-duration $spinner-step-delay infinite);
		}

		&.left, &.right {
				width: $spinner-diamond-size * $spinner-diamond-scale;
				height: $spinner-diamond-size * $spinner-diamond-scale;

				.no-csstransforms3d & {
						width: $spinner-diamond-size * (2 - $spinner-diamond-scale) * $spinner-diamond-scale;
						height: $spinner-diamond-size * (2 - $spinner-diamond-scale) * $spinner-diamond-scale;
				}
		}

		> span {
				display: block;
				position: absolute;
				top: 0; bottom: 0;
				left: 0; right: 0;
				border: 1px solid $spinner-color;
				@include transform(rotate(45deg));
		}

		&.left {
				@include animation-delay(0ms);

				> span {
						@include transform(translateX(-90%) rotate(45deg));
				}
		}

		&.right {
				@include animation-delay($spinner-step-delay * 2);

				> span {
						@include transform(translateX(90%) rotate(45deg));
				}
		}
}

// 2d animation keyframes

$spinner-2d-start: none;
$spinner-2d-l: translateX(-100%) scale($spinner-diamond-scale);
$spinner-2d-r: translateX(100%) scale($spinner-diamond-scale);
$spinner-2d-end: none;

@-webkit-keyframes spinDiamond {
		0% {
				@include transform($spinner-2d-start);
				@include animation-timing-function(ease-in);
		}
		30% {
				@include transform($spinner-2d-l);
				opacity: 0;
		}
		50% {
				@include transform($spinner-2d-r);
				@include animation-timing-function(ease-out);
				opacity: 0;
		}
		80% {
				@include transform($spinner-2d-end);
		}
		100% {
				@include transform($spinner-2d-end);
		}
}

@-moz-keyframes spinDiamond {
		0% {
				@include transform($spinner-2d-start);
				@include animation-timing-function(ease-in);
		}
		30% {
				@include transform($spinner-2d-l);
				opacity: 0;
		}
		50% {
				@include transform($spinner-2d-r);
				@include animation-timing-function(ease-out);
				opacity: 0;
		}
		80% {
				@include transform($spinner-2d-end);
		}
		100% {
				@include transform($spinner-2d-end);
		}
}

@keyframes spinDiamond {
		0% {
				@include transform($spinner-2d-start);
				@include animation-timing-function(ease-in);
		}
		30% {
				@include transform($spinner-2d-l);
				opacity: 0;
		}
		50% {
				@include transform($spinner-2d-r);
				@include animation-timing-function(ease-out);
				opacity: 0;
		}
		80% {
				@include transform($spinner-2d-end);
		}
		100% {
				@include transform($spinner-2d-end);
		}
}

// 3d animation keyframes

$spinner-3d-start: rotateY(0deg) translateZ($spinner-z-offset) rotateY(0deg);
$spinner-3d-mid: rotateY(-180deg) translateZ($spinner-z-offset) rotateY(180deg);
$spinner-3d-end: rotateY(-360deg) translateZ($spinner-z-offset) rotateY(360deg);

@-webkit-keyframes spinDiamond3d {
		0% {
				@include transform($spinner-3d-start);
				@include animation-timing-function(ease-in);
		}
		30% {
				@include transform($spinner-3d-mid);
				opacity: 0;
		}
		50% {
				@include transform($spinner-3d-mid);
				@include animation-timing-function(ease-out);
				opacity: 0;
		}
		80% {
				@include transform($spinner-3d-end);
		}
		100% {
				@include transform($spinner-3d-end);
		}
}

@-moz-keyframes spinDiamond3d {
		0% {
				@include transform($spinner-3d-start);
