}
.popover {
	@include blockRel;
	@include transition(unquote('.3s opacity ease-in-out'));
	background-color: $c-background;
	border: 1px solid $c-border;
	color: $c-text;
	margin: 0 auto;
	opacity: 0;
	top: 20px;

	&.is-visible {
		opacity: 1;
	}
}
.header-cartPopover {
	@include blockFix;
	@include transition(unquote('.3s opacity ease-in-out'));

	border: 1px solid $c-border;
	background-color: rgba($c-background, .97);
	color: $c-text;
	opacity: 0;
	right: 20px;
	top: 65px;
