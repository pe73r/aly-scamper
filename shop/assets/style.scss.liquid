/**
	* Mosaic Section
	*/
.section-mosaic {
	padding: $gutter/2;
	.block { padding: $gutter/2; }
	.block--left {
		@media screen and (max-width: ($med - 1)) {
			display: flex;
			flex-direction: column;

			.mosaic__blocks {
				order: 1;
				width: 100%;
			}

			.mosaic__content {
				order: 2;
				width: 100%;
			}
		}
	}

	button.slick-next,
	button.slick-prev {
		@media screen and (max-width: ($med - 1)) {
			display: flex !important;
			visibility: visible;
		}
	}
}

.mosaic__content {
	display: flex;
	align-items: center;
	justify-content: center;
	text-align: center;
	height: 100%;

	@media screen and (min-width: $med) {
		.mosaic__blocks--one & { height: 50%; }
		.mosaic__blocks--two & { height: 60%; }
		.mosaic__blocks--three & { height: 60%; }
		.mosaic__blocks--four & { height: 40%; }
	}

	@media screen and (max-width: ($med - 1)) {
		padding: 30px 0 15px;
	}

	.button--light { border-color: {{ settings.color_button_bg }}; }
}

.mosaic__content-container {
	padding: 0 100px;

	@media screen and (max-width: ($xxl - 1)) {
		padding: 0 $gutter;
	}
}

.mosaic__heading {
	font-size: $font_base_size * 3.6;

	@media screen and (max-width: ($med - 1)) {
		font-size: $font_base_size * 3;
	}

	.mosaic__blocks--four & {
		font-size: $font_base_size * 2.9;
		margin-bottom: 0.18em;
	}
}

.mosaic__text {
	margin: 0 auto 16px;
	font-size: $font_bodytext_size * 1.12;
	line-height: 1.67;

	.mosaic__blocks--four & {
		font-size: $font_bodytext_size * 0.96;
		letter-spacing: -0.01em;
	}
}

.mosaic__image {
	position: relative;
	height: calc(100vh - #{$gutter}*2);

	.block--default-height & {
		height: calc(75vh - #{$gutter}*2);
	}

	@media screen and (max-width: ($lg - 1)) {
		height: calc(50vh - #{$gutter}*2);

		.block--default-height & {
			height: calc(50vh * 0.75 - #{$gutter}*2);
		}
	}
}

@media screen and (min-width: $med) {
	.mosaic__image--one { min-height: 496px; }
	.mosaic__image--two,
	.mosaic__image--three { min-height: 426px; }
	.mosaic__image--four,
	.mosaic__image--five { min-height: 526px; }
}

/*
 * Mosaic Blocks
 */
@media screen and (min-width: $med) {
	.mosaic__blocks-wrapper {
		height: calc(100vh - #{$gutter}*2);

		.block--default-height & { height: calc(75vh - #{$gutter}*2); }
	}
	.mosaic__blocks--one { min-height: 496px; }
	.mosaic__blocks--two,
	.mosaic__blocks--three { min-height: 426px; }
	.mosaic__blocks--four,
	.mosaic__blocks--five { min-height: 526px; }

	.mosaic__blocks--five {
		.mosaic__block {
			width: calc(50% - #{$gutter});
			&:first-child { width: calc(100% - #{$gutter}); }
		}
	}

	.block--images-only {
		.mosaic__blocks--four {
			.mosaic__block {
				&:nth-child(2),
				&:nth-child(3) {
					width: calc(40% - #{$gutter});
				}

				&:nth-child(1),
				&:nth-child(4) {
					width: calc(60% - #{$gutter});
				}
			}
		}

		.mosaic__blocks--five {
			.mosaic__block {
				&:nth-child(1) {
					width: calc(100% - #{$gutter});
				}
				&:nth-child(3),
				&:nth-child(4) {
					width: calc(40% - #{$gutter});
				}

				&:nth-child(2),
				&:nth-child(5) {
					width: calc(60% - #{$gutter});
				}
			}
		}
	}

	.block--no-text {
		.mosaic__blocks--two .mosaic__block {
			width: calc(100% - #{$gutter});
			height: calc(50% - #{$gutter});
		}
		.mosaic__blocks--three .mosaic__block {
			width: calc(50% - #{$gutter});
			height: calc(50% - #{$gutter});

			&:first-child { width: calc(100% - #{$gutter}); }
		}
		.mosaic__blocks--four .mosaic__block { height: calc(50% - #{$gutter}); }
		.mosaic__blocks--five .mosaic__block { height: calc(33.3333% - #{$gutter}); }
	}
}

.mosaic__blocks--arrows-white {
	button.slick-next::after,
	button.slick-prev::after {
		color: #fff;
	}
}

.mosaic__blocks {
	@media screen and (min-width: $med) {
		display: flex;
		flex-wrap: wrap;
		float: left;
		width: calc(100% + #{$gutter});
		margin: -$gutter/2;

		.mosaic__blocks--one & { height: calc(50% + #{$gutter}); }
		.mosaic__blocks--two & { height: calc(40% + #{$gutter}); }
		.mosaic__blocks--three & { height: calc(40% + #{$gutter}); }
		.mosaic__blocks--four & { height: calc(60% + #{$gutter}); }
		.mosaic__blocks--five & { height: calc(100% + #{$gutter}); }
		.block--no-text & { height: calc(100% + #{$gutter}); }
	}
}

.mosaic__block {
	position: relative;
	height: calc(100% - #{$gutter});

	@media screen and (max-width: ($med - 1)) {
		height: 50vh;
	}

	@media screen and (min-width: $med) {
		margin: $gutter/2;

		.mosaic__blocks--one & { width: calc(100% - #{$gutter}); }
		.mosaic__blocks--two & { width: calc(50% - #{$gutter}); }
		.mosaic__blocks--three & { width: calc(33.33% - #{$gutter}); }
		.mosaic__blocks--four & {
			width: calc(50% - #{$gutter});
			height: calc(50% - #{$gutter});
		}

		.mosaic__blocks--four .mosaic__blocks--split & {
			&:nth-child(1),
			&:nth-child(2),
			&:nth-child(3),
			&:nth-child(4) {
				width: calc(50% - #{$gutter});
			}
		}

		.mosaic__blocks--five .mosaic__blocks--split & {
			&:nth-child(2),
			&:nth-child(3),
			&:nth-child(4),
			&:nth-child(5) {
				width: calc(50% - #{$gutter});
			}
		}
	}
}

.mosaic__block--video {
	.icon-play {
		position: absolute;
		top: 50%;
		left: 50%;
		display: inline-block;
		width: auto;
		font-size: 50px;
		line-height: 1;
		text-align: center;
		color: #fff;
		@include transform(unquote('translate(-50%, -50%)'));

		&.style--dark {
			color: #222;
		}
	}
}

.mosaic__block-image,
.mosaic__block-product {
	position: relative;
	height: 100%;
	display: block;
}

.mosaic__block-bg,
.mosaic__image-bg {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	background-position: center center;
	background-repeat: no-repeat;
	background-size: cover;
	overflow: hidden;
}

.mosaic__image-bg--no-image,
.mosaic__block-bg--no-image,
.mosaic__block-product-image--no-image {
	.svg-placeholder {
		position: absolute;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;

		svg {
			max-width: 200%;
		}
	}
}

.mosaic__block-product-image--no-image {
	.svg-placeholder {
		background: none;

		svg {
			min-height: 220px;
		}
	}
}

.mosaic__block-text,
.mosaic__image-text {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	padding: 16px;
	display: flex;
	align-items: flex-end;
	justify-content: flex-start;
