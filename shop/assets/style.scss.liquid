/**
 * Selected image flag.
 */
.product-selected-flag {
	@include transition(unquote('opacity 0.3s ease'));
	background-color: $c-secondary;
	color: #FFFFFF;
	opacity: 0;
	pointer-events: none;
	&.is-visible {
		opacity: 1;
		pointer-events: auto;
	}
}
.product-selected-mark {
	@include transition(unquote('opacity 0.3s ease'));
	@include blockAb;
	background-color: $c-secondary;
	border-radius: 100px;
	opacity: 0;
	pointer-events: none;
	top: 15px; left: 15px;
	width: 30px; height: 30px;
	z-index: 1;
	&::after {
		@include blockAb;
		border-bottom: 3px solid #FFFFFF;
		border-left: 3px solid #FFFFFF;
		content: '';
		height: 6px;
		top: 9px; left: 8px;
		-webkit-transform: rotate(-45deg);
		-moz-transform: rotate(-45deg);
		ms-transform: rotate(-45deg);
		transform: rotate(-45deg);
		width: 12px;
	}
	&.is-visible {
		opacity: 1;
		pointer-events: auto;
	}
}
.product-image-inner {
	@include blockAb;
	bottom: 0; left: 0; right: 0; top: 0;
	overflow: hidden;
	.product-image--fit & {
		text-align: center;
		&::after {
			@include inlineRel;
			content: '';
			height: 100%;
			vertical-align: middle;
		}
		.product-image-img {
