 * Two-up text module.
 */
.two-up-module {
	padding-bottom: 45px;
	text-align: center;
	.row-two-up {
		overflow: hidden;
		position: relative;
		@media (min-width: $med) {
			height: 400px;
		}
	}
	.col-image {
		height: 0;
		padding-top: 50%;
		position: relative;
		.two-up-image {
			background-position: 50% 50%;
			background-repeat: no-repeat;
			background-size: cover;
			bottom: 0;
			left: 0;
			position: absolute;
			right: 0;
			top: 0;
		}
	}
	.col-text {
		padding: 45px 20px;
		h2 {
			margin-bottom: 10px;
			@media (min-width: $sm){
				margin-bottom: 15px;
			}
		}
	}
	@media (min-width: $lg) {
		.col {
			/**
			 * Was a very small gap between these for some reason.
			 */
			overflow: hidden;
			width: 50.1%;
		}
		.col-image {
			bottom: 0;
			height: auto;
			padding: 0;
			position: absolute;
			top: 0;
		}
		.col-text {
			padding: 80px 35px;
			> * {
				max-width: auto;
			}
		}
		&.image-left {
			.col-image {
				left: 0;
			}
			.col-text {
				float: right;
			}
		}
		&.image-right {
			.col-image {
				right: 0;
			}
			.col-text {
				float: left;
			}
		}
	}
	@media (min-width: $lg){
		.col-text {
			padding: 80px 90px;
		}
	}
}

.account-form {
	.no-js & {
		display: block !important;
		margin-left: auto;
		margin-right: auto;
	}

	h2 {
		margin-bottom: 20px;
	}
	h3 {
		margin-bottom: 10px;
		font-size: $font_size_h2;
		font-family: $general_font;
		font-weight: $general_font_weight;
		font-style: $general_font_style;
	}
	input[type="text"],
	input[type="email"],
	input[type="password"],
	input[type="tel"] {
		font-size: $font_size_h4;
		background-color: white;
		border: 1px solid $c-border;
		width: 100%;
	}

	input[type="hidden"] {
    display: none;
}

	select {
		font-size: $font_size_h4;
		background-color: #FFFFFF;
		border: 1px solid $c-border;
		width: 100%;
	}

	.errors {
		margin: 10px 0;
		a { text-decoration: underline; }
	}

	.account-form-links {
		a {
			display: block;
			margin: 8px auto;
		}

	}

	.button,
	.account-form-submit {
		display: inline-block;
		vertical-align: middle;
		float: left;
			@media (max-width: $sm){
				text-align: center;
				width: 100%;
			}
		}
		.guest-login-submit {
			margin-left: 10px;
			@media (max-width: $sm){
				margin-left: 0px;
				margin-top: 10px;
			}
			&.button--light{
				border: 1px solid {{setting.color_button_bg}};
			}
		}
}

.js-editAddress, .js-addAddress {
	span.validation {
		@include h6;
		color: $c-secondary;
		padding-top: 7px;
		display: block;
		height: 0;
		overflow: hidden;
		@include transition(unquote('height .3s'));
	}
	span.validation.validation--showup {
		height: 20px;
	}
}

.checkboxBlock {
	input[type='checkbox'] {
		display: inline-block;
		margin-right: 10px;
	}

	label {
		display: inline;
	}
}

// Wrapper for /account page
.account-home {
	margin: 0 auto 50px;

	h3 {
		font-size: $font_size_h2;
		font-family: $general_font;
		font-weight: $general_font_weight;
		font-style: $general_font_style;
		margin-bottom: 10px;
	}

	.account-orders, .account-details {
		vertical-align: top;
	}

	.account-title {
		margin-bottom: 2em;
		a {opacity: .6;}
	}
}

/* The Account: Link at the tops of the inner account pages
 */
.account-crumbs {
	margin-bottom: 2em;
}
a.account-breadcrumb {
	@include transition(.2s opacity);
	color: $c-text;

	&:hover {
		opacity: 0.8;
	}
}

// link to all addresses
a.account-addresses {
	@include transition(.2s opacity);
	color: $c-text;

	&:hover {
		opacity: 0.8;
	}
}

/* Global Table styles for /account pages
 */
.account-table {

	.d-table-cell {
		padding: 15px 0;
		border-bottom: 1px solid $c-border;
		text-align: center;

		&.no-border {
			:cn
			:Cn
		}
	}

	@media (max-width: ($med - 1)){
		.d-table-header {
			display: none;
		}
		.d-table-cell {
			float: left;
			text-align: left;
			border-bottom: 0px solid $c-border;
			border-top: 1px solid $c-border;
		}
		.d-table-row {
			border-bottom: 1px solid $c-border;

			&::before,
				&::after {
					content: " ";
					display: table;
				}

				&::after {
					clear: both;
				}
		}

		// since all cells are left aligned on mobile
		.u-right {
			text-align: left;
		}
	}
}

/* Order table on /account page
 */
.account-home-table {
	.order-number {
		text-align: left;

		a {
			text-decoration: underline;
			@include transition(0.2s opacity);
			color: $c-text;
			&:hover {
				opacity: 0.8;
			}
		}
	}
	.order-total {
		text-align: right;
	}

	@media (max-width: ($med - 1)){
		.order {
			margin: 30px 0 0;
		}
		.d-table-cell {
			width: 50%;
		}
		.order-number {
			border-top: 0px solid $c-border;
			width: 100%;

			h4 {
				color: $c-text;
			}

			h4, a {
				display: inline !important;
			}
		}
		.order-total {
			text-align: left;
		}
	}
}
.account-details {
	@media (max-width: ($lg - 1)){
		margin-top: 50px;
	}
}

/*
 * Orders Page
 */
.order-table {
