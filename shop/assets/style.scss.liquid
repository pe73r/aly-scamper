// Assign font stack
@mixin h1 {
	font-family: $header_font;
	font-weight: $header_font_weight;
	font-style: $header_font_style;
	font-size: $font_size_h1;
	line-height: 1.1;

	&.size--small {
		font-size: $font_size_h1 * $size_scale_small;
	}
	&.size--large {
		font-size: $font_size_h1 * $size_scale_large;
	}
	&.size--xlarge {
		font-size: $font_size_h1 * $size_scale_xlarge;
	}

	@media screen and (max-width: ($sm - 1)) {
		font-size: $font_size_h1_sm;

		&.size--small {
			font-size: $font_size_h1_sm * $size_scale_small;
