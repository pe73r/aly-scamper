.order-table-summary {
	margin: 30px 0;

	@media (max-width: ($med - 1)){
		.d-table-cell {
			width: 50%;
		}
	}
	@media (max-width: $sm){
		h2.med_show {
			border-bottom: 1px solid $c-border;
			padding-bottom: 15px;
			margin-bottom: 0;
		}
