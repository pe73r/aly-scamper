.icon-menu {
	position: relative;
	width: 24px;
	height: 19.58px;
	@include transition(.3s);

	.diamond-bar {
		position: absolute;
		top: -12.71px;
		left: -10.5px;
		width: 45px;
		height: 43px;
		background-color: transparent;
		@include transition(.3s);
	}

	.icon-bar,
	.icon-bar:before,
	.icon-bar:after {
		position: absolute;
		display: block;
		width: 24px;
		height: 2px;
		border-radius: 2px;
		@include transition(.3s);
	}

	.icon-bar:before,
	.icon-bar:after {
		content: "";
	}

	.icon-bar {
		top: 7.92px;
	}

	.icon-bar:before {
		top: -7.92px;
	}

	.icon-bar:after {
		top: 7.92px;
	}

	&.open {

		.diamond-bar {
			@include transform(rotate(45deg));
			background-color: $c-header-icon-bg;
		}

		.icon-bar:before,
		.icon-bar:after {
			top: 0;
		}

		#hamburger-bar {
			background-color: transparent;
		}

		.icon-bar:before {
			@include transform(rotate(45deg));
		}

		.icon-bar:after {
			@include transform(rotate(-45deg));
		}
	}
}

