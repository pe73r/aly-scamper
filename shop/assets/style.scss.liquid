/*
 * Product Grid Slider
 */
.collection-slider {
	margin-bottom: 30px;

	@media screen and (max-width: ($med - 1)) {
		width: 100%;

		&.row { width: 100%; }
	}

	.collectionBlock {
		padding: 22px 5px 0;

		&.quickView--is-active,
		&.quickView--is-visible {
			position: static !important;
		}

		@media screen and (max-width: ($med - 1)) {
			width: 100%;
			min-width: 100%;
			opacity: .3;
			@include transition(unquote('opacity .35s ease'));
			-webkit-backface-visibility: hidden;
			&.is-selected { opacity: 1; }
		}
	}

	button.slick-prev,
	button.slick-next {
		display: flex !important;
		top: 50%;
		margin-top: 5px;

		&::after {
			font-size: .9rem;
			line-height: normal;

			@media screen and (max-width: ($lg - 1)) {
				font-size: 0.65rem;
			}

			@media screen and (max-width: ($med - 1)) {
				font-size: 0.45rem;
			}
		}
	}

	button.slick-prev {
		left: -55px;

		&::after {
			content: '\e904';
		}

		@media screen and (max-width: ($med - 1)) {
			left: -35px;
		}
	}

	button.slick-next {
		right: -55px;

		&::after {
			content: '\e905';
		}

		@media screen and (max-width: ($med - 1)) {
			right: -35px;
		}
	}

	.slick-slide.has-quickView {
		position: static !important;
		will-change: unset !important;
	}

	.quickView-wrap {
		width: 85vw;
		right: auto;
	}

	.collectionBlock-info {
		padding-bottom: 0;
	}
}

.collection-slider__wrapper {
	margin-left: -5px;
	margin-right: -5px;
	@media screen and (max-width: ($med - 1)) {
		width: 66%;
		backface-visibility: hidden;
		-webkit-backface-visibility: hidden;
		margin: 0 auto;

		.flickity-viewport {
			overflow: visible !important;
		}
	}
}

