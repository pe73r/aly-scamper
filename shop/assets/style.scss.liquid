	@media screen and (max-width: ($med - 1)) {
		font-size: $font_bodytext_size * 1.3;
	}
}

.mosaic__block-link {
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 100%;
}

.mosaic__block-product-image {
	position: relative;
