/**
 * These are the individual grid items.
 */
.collectionBlock {
	/**
	 * Sets the featured products on the Collection page.
	 */
	 @include blockRel;

	&.animated {
		animation-fill-mode: none;
		-webkit-animation-fill-mode: none;
	}

	.price--hover {
		transform: translateY(100%);
		opacity: 0;
		@include transition(unquote('transform .3s, opacity .3s'));
		@media screen and (max-width: ($med - 1)){
			opacity: 1;
			transform: none;
		}
	}


	&:hover {
		.price--hover {
			transform: translateY(0%);
			opacity: 1;
		}
	}

	.has-featured-products & {
		&:nth-of-type(-n+2) {
			width: 100%;
			@media screen and (min-width: $med){
				width: calc(50% - #{$gutter}/2) !important; // $gutter value in _grid.scss
			}
		}
	}
	@include transition(opacity .2s);
	.ajax-sorting & { opacity: 0; }
}
/**
 * This controls the size of image.
 */
.collectionBlock-image {
	@include blockRel;
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
	padding-top: {{ settings.image_aspect_ratio | times: 100 | append: '%' }};

	.collectionBlock-hover {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		background-color: #fff;
		background-position: center;
		background-repeat: no-repeat;
		background-size: contain;
		opacity: 0;
		@include transition(unquote('opacity .3s'));
		@include transition(unquote('visibility .3s'));

		.svg-placeholder {
			top: 50%;
			left: 50%;

			svg {
				opacity: .5;
			}
		}
	}
	.collectionBlock-hover--placeholder {
		background: #eee;
	}

	& > .collectionBlock-hover::before {
		content: '';
		position: absolute;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		background-color: $c-background;
	}

	@media screen and (min-width: $med){
		&:hover {
			.collectionBlock-hover,
			.collectionBlock-image-inner { opacity: 1; }
		}
	}

	@media screen and (max-width: ($sm - 1)) {
		margin-left: auto;
		margin-right: auto;
	}
}
.collectionList-row { padding: 0 5px; }
.row--mosaic {
	padding: 0 5px;

	&::after {
		content: '';
		display: table;
		clear: both;
		height: 0;
		line-height: 0;
	}

	.collectionBlock__vertical-description {
		position: absolute;
		top: 50%;
		left: calc(50% - 20px);
		width: 100%;
		height: 1px;
		display: flex;
		align-items: center;
		justify-content: center;
		@include transform(unquote('rotate(90deg)'));

		p {
			margin: 0;
			white-space: nowrap;
			overflow: hidden;
			text-overflow: ellipsis;
			text-align: center;
			text-transform: uppercase;
			color: inherit;
			letter-spacing: 1px;
			max-width: calc(45vh - 40px);
		}

		@media screen and (max-width: ($xl - 1)) {
			display: none;
		}
	}

	@media screen and (max-width: ($med - 1)) {
		.collectionBlock {
			float: left;
			width: 100%;
		}

		.collectionBlock-image {
			padding-top: 100%;
			height: 0;
		}

		.collectionBlock__vertical-description {
			display: none;
		}

		&.row--2-blocks {
			.collectionBlock {
				width: 50%;
			}
		}

		&.row--3-blocks {
			.collectionBlock {
				width: 50%;

				&:nth-child(1) {
					width: 100%;

					.collectionBlock-image {
						padding-top: 50%;
					}
				}
			}
		}

		&.row--4-blocks {
			.collectionBlock {
				width: 50%;

				&:nth-child(1),
				&:nth-child(4) {
					width: 100%;

					.collectionBlock-image {
						padding-top: 50%;
					}
				}
			}
		}

		&.row--5-blocks {
			.collectionBlock {
				width: 50%;

				&:nth-child(1),
				&:nth-child(4) {
					width: 100%;

					.collectionBlock-image {
						padding-top: 50%;
					}
				}

				&:nth-child(5) {
					width: 100%;

					.collectionBlock-image {
						padding-top: 100%;
					}
				}
			}
		}
	}

	@media screen and (max-width: $xs) {
		.collectionBlock {
			width: 100%;
		}
	}

	@media screen and (min-width: $med) {
		position: relative;
		height: calc(100vh - 60px);

		&.row--default-height {
			height: 75vh;
			min-height: 480px;

			.collectionBlock__vertical-description {
				p {
					max-width: calc(35vh - 40px);
				}
			}

			&.row--3-blocks,
			&.row--4-blocks,
			&.row--5-blocks {
				.collectionBlock:last-child {
					.collectionBlock__vertical-description {
						p {
							max-width: calc(50vh - 50px);
						}
					}
				}
			}
		}

		.collectionBlock {
			float: left;
			position: relative;
			width: 100%;
			height: 100%;
		}

		.collectionBlock-image {
			height: 100%;
			padding-top: 0;
		}

		.collectionBlock-info {
			padding: 30px;

			div {
				width: 100%;
			}

			&.text-position--bottom_right {
				padding-right: 50px;
			}
		}

		&.row--1-blocks {
			.collectionBlock {
				.collectionBlock__vertical-description {
					p {
						max-width: calc(100vh - 100px);
					}
				}
			}
		}
		&.row--2-blocks {
			.collectionBlock {
				width: 50%;
				height: 100%;

				.collectionBlock__vertical-description {
					p {
						max-width: calc(100vh - 100px);
					}
				}
			}
		}
		&.row--3-blocks {
			padding-right: 50%;

			.collectionBlock {
				width: 100%;
				height: 50%;

				&:last-child {
					width: calc(50% - 5px);
					height: 100%;
					position: absolute;
					top: 0;
					right: 5px;
				}
			}
		}
		&.row--4-blocks {
			padding-right: 50%;

			.collectionBlock {
				width: 100%;
				height: 50%;

				&:nth-child(2),
				&:nth-child(3) {
					width: 50%;
				}

				&:last-child {
					width: calc(50% - 5px);
					height: 100%;
					position: absolute;
					top: 0;
					right: 5px;
				}
			}
		}
		&.row--5-blocks {
			padding-right: 40%;

			.collectionBlock {
				height: 50%;
				&:nth-child(1),
				&:nth-child(4) {
					width: 66.66%;
				}
				&:nth-child(2),
				&:nth-child(3) {
					width: 33.33%;
				}

				&:last-child {
					width: calc(40% - 5px);
					height: 100%;
					position: absolute;
					top: 0;
					right: 5px;
				}
			}
		}

		&.row--3-blocks,
		&.row--4-blocks,
		&.row--5-blocks {
			.collectionBlock:last-child {
				.collectionBlock__vertical-description {
					p {
						max-width: calc(100vh - 100px);
					}
				}
			}
		}
	}

	@media screen and (max-width: ($lg - 1)) and (min-width: $med) {
		height: auto;
		min-height: 0;

		&.row--1-blocks,
		&.row--2-blocks {
			.collectionBlock {
				height: 50vh;
				min-height: 480px;
			}
		}

		&.row--3-blocks,
		&.row--4-blocks,
		&.row--5-blocks {
			padding-right: 5px;

			.collectionBlock {
				height: 25vh;
				min-height: 240px;

				.collectionBlock__vertical-description {
					p {
						max-width: calc(25vh - 40px);
					}
				}

				&:last-child {
					width: 100%;
					height: 50vh;
					min-height: 480px;
					position: relative;
					right: 0;
				}
			}
		}

		&.row--5-blocks {
			.collectionBlock:last-child {
				right: 0;
			}
		}

		&.row--default-height {
			height: auto;
			min-height: 0;

			.collectionBlock {
				height: 35vh;
				min-height: 240px;
			}

			&.row--1-blocks,
			&.row--2-blocks {
				.collectionBlock {
					height: 75vh;

					.collectionBlock__vertical-description {
						p {
							max-width: calc(75vh - 50px);
						}
					}
				}
			}
			&.row--3-blocks,
			&.row--4-blocks,
			&.row--5-blocks {
				.collectionBlock {
					&:last-child {
						height: 75vh;
						min-height: 480px;
					}
				}
			}
		}
	}

	@media screen and (max-width: $xs) {
		.button {
			padding: 14px 18px;
		}
	}
}
