.mobile-zoom-overlay {
	@include blockFix;
	@include transition(unquote('opacity .3s'));
	background-color: rgba($c-background, .9);
	bottom: 100%;
	left: 0;
	opacity: 0;
	right: 0;
	top: 0;
	z-index: 1000;

	&.is-visible {
		bottom: 0;
		opacity: 1;
	}
	&.is-hiding {
		opacity: 0;
	}
	img {
		@include blockRel;
		@include transition(unquote('opacity .3s'));
		margin: 0 auto;
		max-width: 95%;
		opacity: 0;
		z-index: 0;
		&.fade-in {
			opacity: 1;
		}
	}
	.mobile-zoom-close {
		@include blockAb;
		background-color: $c-text;
		color: $c-background;
		cursor: pointer;
		display: none;
		height: 50px;
		right: 0;
		text-align: center;
		top: 0;
		width: 50px;
		z-index: 1;
		.icon-close {
			font-size: 20px;
			line-height: 50px;
		}
		.image-hover-light & {
			background-color: $light-overlay;
			color: hover-color($light-overlay, $c-text);
		}
	}

}

.is-visible .mobile-zoom-close {
	display: block;
}

/*
 * Footer
 */
.site-footer {
	color: $c-footer-text;
	background-color: $c-footer-bg;

	ul, li {
		list-style-type: none;
	}

	.footer-inner {
		padding-top: 40px;
		padding-bottom: 36px;
