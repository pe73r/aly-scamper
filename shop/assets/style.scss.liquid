				&::after {
					@include transform(unquote('translate(-50%, -50%) rotate(-90deg)'));
					opacity: 0;
				}
			}

			&::before {
				width: 100%;
				opacity: 0;
				@include transition(unquote('width 0s linear 0s, opacity .5s ease'));
			}

			&::after {
				width: 0;
				opacity: .35;
				transition: none;
			}
		}

		.product-tab-content {
			margin: 10px 0;
			height: auto;
			opacity: 1;
