	.product-tab-title {
		@include blockRel;
		cursor: pointer;
		padding: 0px 40px 0 0;

		.icon-toggle {
			position: absolute;
			top: 50%;
			right: 0;
			width: 9px;
			height: 1px;

			&::before,
			&::after {
				content: '';
				position: absolute;
				left: 50%;
				top: 50%;
				width: 100%;
				height: 100%;
				background: $c-text;

				@include transition(unquote('transform .5s ease, opacity .5s ease'));
			}

			&::before {
				@include transform(unquote('translate(-50%, -50%) rotate(90deg)'));
			}

			&::after {
				@include transform(unquote('translate(-50%, -50%)'));
			}
		}

		&::before,
		&::after {
			content: '';
			position: absolute;
			left: 0;
			top: -10px;
			bottom: -10px;
			width: 0;
			opacity: .35;
			background: $c-border;
