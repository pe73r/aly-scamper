/* Use a row to specify width and .inline or .float options
 * to set up a wrapper for grids.
 */
@mixin row {
	@include blockRel;

	margin: 0 auto;
	max-width: 100%;
	width: 100%;
	z-index: 1;

	&::before,
	&::after {
		content: " "; /* 1 */
		display: table; /* 2 */
	}

	&::after {
		clear: both;
	}

	&.row-sm {
			width: 90%;

			@media screen and (min-width: $xs) {
					width: 75%;
			}
			@media screen and (min-width: $med) {
					width: 60%;
			}
	}

	&.row-med {
			width: 90%;

			@media screen and (min-width: $xs) {
					width: 80%;
			}
			@media screen and (min-width: $med) {
					width: 70%;
			}
	}

	&.row-lg {
			width: 90%;

			@media screen and (min-width: $xs) {
					width: 87.5%;
			}
			@media screen and (min-width: $med) {
					width: 85%;
			}
	}

	&.inline {
			text-align: justify;

			// removes clearfix from default .row
			&::before {
					display: none;
			}

			&::after {
					@include inlineRel;

					content: '';
					width: 100%;
			}

			&.center {
					text-align: center;
			}

			.block {
					@include inlineRel;

					float: none;
			}
	}

	&.va-m .block {
			vertical-align: middle;
	}

	// Vertically middle align child, e.g. ".content"
	&.vm-child {
			&::after {
					@include inlineRel;

					content:'';
					height: 50%;
					width: 0px;
			}
	}
}
.row {
	@include row;
}

