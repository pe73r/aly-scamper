/**
	* Swatches Styles
	*/
{% assign swatch_width = '22px' %}
{% assign swatch_height = '22px' %}
.swatch {
	display: flex;
	flex-wrap: wrap;

	@media screen and (max-width: ($med - 1)){
		justify-content: center;
	}

	.header {
		width: 100%;
		color: $c-text;
		text-align: left;
		margin-bottom: 10px;

		@media screen and (max-width: ($med - 1)) {
			text-align: center;
		}
	}

	.header__value {
		display: none;
		color: $c-text;
		&::before {
			content: ': ';
			color: rgba($c-text, .5);
		}
	}

	input {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		opacity: 0;
		border-radius: 50%;
		z-index: 10;
		display: block;
		cursor: pointer;

		&:checked ~ span {
			border-color: #000;
		}
	}

	label {
		position: relative;
		float: left;
		height: {{ swatch_height }} !important;
		min-width: {{ swatch_width }} !important;
		margin: 0;
		font-size: 13px;
		text-align:center;
		line-height: {{ swatch_height }};
		white-space: nowrap;
		text-transform: uppercase;
		display: flex;
		align-items: center;
		justify-content: center;

		span {
			position: absolute;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
			border-radius: 50%;
			background-color: #ddd;
			background-position: center center;
			background-size: cover;
			background-repeat: no-repeat;
			border: 1px solid transparent;
			@include transition(unquote('border .2s'));
		}
	}

	.swatch-element {
		position: relative;
		margin: 0px 18px 18px 0;
		-webkit-transform: translateZ(0); /* webkit flicker fix */
		-webkit-font-smoothing:antialiased; /* webkit text rendering fix */

		label {
			padding: 0 10px;
			cursor: pointer;
		}

		&.color {
			label {
				padding: 0;
			}
		}

		&.soldout {
			label {
				filter: alpha(opacity=60); /* internet explorer */
				-khtml-opacity: 0.6;      /* khtml, old safari */
				-moz-opacity: 0.6;       /* mozilla, netscape */
				opacity: 0.6;           /* fx, safari, opera */
			}
		}

		&.white {
			input:not(:checked) ~ span {
				border-color: rgba(0, 0, 0, .35);
			}
		}

		&[class*="black"] {
			input:checked ~ span {
				box-shadow: inset 0 0 0 1px #fff;
			}
		}

		@media screen and (max-width: ($med - 1)) {
			float: none;
			display: inline-block;
			margin: 0 9px 18px;
		}
	}

	&.error {
		background-color: #E8D2D2!important;
		color: #333 !important;
		padding: 1em;

		p {
			margin: 0.7em 0;

			&:first-child {
				margin-top: 0;
			}

			&:last-child {
				margin-bottom: 0;
			}
		}

		code {
			font-family: monospace;
		}
	}
}

.swatch--square {
	label {
		span {
			border-radius: 0;

			&::after {
				border-radius: 0;
				top: -3px;
				left: -3px;
				right: -3px;
				bottom: -3px;
			}
		}
	}
}

.swatch-link {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	border-radius: 50%;
	overflow: hidden;
	font-size: 0;
	line-height: 0;
	text-indent: -100%;
	display: none;
}

.swatches-fake {
	width: 100%;
	display: flex;
	align-items: center;
	justify-content: center;

	input,
	.header {
		display: none;
	}

	.swatch-element {
		margin: 0 4px;

		label {
			min-width: 12px !important;
			width: 12px !important;
			height: 12px !important;
			border: 0;
			border-radius: 0;
			cursor: default;

			span {
				width: 100%;
				height: 100%;
				border-color: transparent;
				box-shadow: none;
			}
		}

		&[class*="black"] input:checked ~ span {
			box-shadow: none;
		}
	}

	.swatch-link {
		display: block;
	}
}

