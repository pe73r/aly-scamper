}

.site-footer--push {
	@media screen and (min-width: $lg) {
		margin-bottom: 48px;
	}
}

.footer__blocks {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
	margin: 0 -15px;

	@media screen and (max-width: ($lg - 1)) {
		margin: 0 -10px;
	}
	@media screen and (max-width: ($med - 1)) {
		margin: 0;
		flex-direction: column;
		justify-content: flex-start;
		align-items: center;
		text-align: center;
	}
}
.footer__blocks--2 {
	justify-content: flex-start;
}
.footer__block {
	max-width: 20%;
	padding: 0 15px;
	line-height: 1.5;

	@media screen and (max-width: ($lg - 1)) {
		padding: 0 10px;
	}
	@media screen and (max-width: ($med - 1)) {
		max-width: none;
		width: 100%;
		padding: 0;
		margin: 0 0 30px;

		&:last-child { margin-bottom: 0; }
	}

	.footer__blocks--2 & {
		width: 20%;

		@media screen and (max-width: ($med - 1)) {
			width: 100%;
		}
	}
}

.footer__block-title {
	font-size: $font_bodytext_size * 1.14;
	font-weight: $font_weight_medium;
	margin: 0 0 15px;
}
.footer__block-text {
	margin-bottom: 10px;
}

.footer__block-menu-link { margin-bottom: 5px; }

.footer-bottom {
	padding: 28px 0 20px;
	font-size: 12px;
	line-height: 1.5;
	color: rgba($c-footer-text, .7);
	background-color: $c-bottom-bar-bg;

	@media screen and (max-width: ($med - 1)) {
		text-align: center;
	}
}
.footer-bottom__left {
	width: 50%;
	float: left;

	@media screen and (max-width: ($med - 1)) {
		float: none;
		width: 100%;
	}
}
.footer-bottom__right {
	max-width: 50%;
	float: right;

	@media screen and (max-width: ($med - 1)) {
		float: none;
		max-width: none;
	}
}

.footer__logo {
	display: inline-block;
	margin: 5px 0;

	img { display: block; }
}

.footer__payment {
	margin: 0 0 10px;

	@media screen and (max-width: ($med - 1)) {
		margin-bottom: 15px;
	}
}

.footer__text,
.footer__copyright {
	color: inherit;

	p {
		font-size: inherit;
		line-height: inherit;
		margin: 0;
	}

	a { text-decoration: none; }
}

.footer__social {
	margin: 5px 0 0;
	@media screen and (max-width: ($med - 1)) {
		text-align: center;
		margin: 0 0 20px;
	}

	.social-links {
		margin: 0 0 9px;
		white-space: nowrap;
	}
}

.footer__social-link {
	display: inline-block;
	margin-right: 10px;
	font-size: $font_bodytext_size;

	&:last-child { margin-right: 0; }

	[class^="icon-"] {
		text-align: center;
		width: auto;
	}

	a:hover { opacity: .7; }
}

.footer__menu {
	font-size: inherit;
	line-height: inherit;

	ul {
		display: flex;
		flex-wrap: wrap;

		@media screen and (max-width: ($med - 1)) {
			justify-content: center;
			margin-bottom: 10px;
		}
	}

	li {
		margin-right: 15px;

		&:last-child { margin-right: 0; }

		@media screen and (max-width: ($med - 1)) {
