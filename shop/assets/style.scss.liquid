/**
 * Product Images and Layouts
 */
.product-image {
	@include transition(unquote('opacity 0.3s ease-in-out'));
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
	margin-bottom: 40px;
	padding-top: 66%;
	&.zoom--is-loading {
		&::after {
			@include blockAb;
			background-image: url('{{'load-more.gif' | asset_url }}');
			background-position: center;
			background-repeat: no-repeat;
			background-size: 79px 64px !important;
			bottom: 0;
			content: '';
			left: 0;
			right: 0;
			top: 0;
		}
		.product-image-zoom {
			background-color: $dark-overlay;
			.image-hover-light & {
				background-color: $light-overlay;
			}
		}
	}
	&.zoom--is-visible {
		img {
			@include transition(unquote('opacity 0.3s ease'));
			opacity: 1 !important;
		}
		.product-image-zoom {
			background-color: $dark-overlay;
			.image-hover-light & {
				background-color: $light-overlay;
			}
		}
	}
	&.zoom--is-hiding {
