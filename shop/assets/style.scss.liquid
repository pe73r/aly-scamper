}

.errors { color: #c0363a; }

.currency-picker {
	display: inline-block;
	position: relative;
	font-size: $font_base_size;
	font-family: $general_font;
	font-style: $general_font_style;
	font-weight: $general_font_weight;
	color: inherit;
	padding-left: 0;

	select {
		background-color: transparent;
		width: 100%;
		padding-right: 1.5em;
		padding-top: 0;
		padding-bottom: 0;
		color: inherit;

		// Prevent select options to be invisible
		option {
			// IE fix
			color: #000;
			color: initial;
		}
	}

	.selectArrow {
		right: 0;
		height: calc(100% - 2px);
		font-size: $font_base_size;
		color: inherit;
	}
}

.fallback-text {
	text-transform: capitalize;
	position: absolute;
	overflow: hidden;
	clip: rect(0 0 0 0);
	height: 1px;
	width: 1px;
	margin: -1px;
	padding: 0;
	border: 0;
}

.payment-icon-list {
	margin: 0;
	color: $c-footer-text;
	font-size: $font_bodytext_size * 1.5;

	& > li {
		display: inline-block;
		list-style: outside none none;
		margin: 0;

		.payment-icon {
			display: inline-block;
			width: 38px;
			height: 24px;
			vertical-align: middle;
			box-shadow: 0 1px 2px rgba(0, 0, 0, .1);
			border-radius: 3px;
		}
	}

	@media screen and (max-width: $sm) {
		text-align: center;
	}
}

/*
 * Header
 */
.site-header {
	@include blockAb;
	@include transition(unquote('background .2s, left .2s, opacity .15s, transform .3s, height .2s'));
	color: $c-header-text;
	background-color: $c-header-bg;
	left: 0;
	padding: 10px 0;
	top: 0;
	width: 100%;
	z-index: 1000;
	opacity: 0;

	&.template-index {
		color: $c-header-text-home;
		background-color: $c-header-bg-home;
	}

	&.has-scrolled,
	&.header--megamenu-visible {
		.logo-home {
			position: absolute !important;
			visibility: hidden !important;
		}
		.logo-other {
			position: relative !important;
			visibility: visible !important;
		}
	}

	@media screen and (min-width: $lg) {
		&.is-standard {
			position: fixed;
			top: 0;
			padding: 0;
			height: 120px;

			&.header--megamenu-visible {
				.nav-standard,
				.main-logo__text,
				#cartTotal .cartToggle-items,
				.currency-picker { color: $c-header-text; }

				.header-icons svg path { fill: $c-header-text; }
			}

			.container {
				top: 0;
				@include transition(unquote('background .2s ease, top .2s ease, padding .2s ease'));
			}

			.menuToggle {
				display: none;
			}

			.header-fix-cont {
				position: absolute;
				z-index: auto;
				top: 0px;
				bottom: 0;
				height: 120px;
				@include transition(unquote('height .2s ease'));
			}

			.main-logo {
				position: relative;
				font-size: 22px;
				align-items: center;
			}

			.nav-standard {
				display: flex;

				@media screen and (max-width: ($lg - 1)) {
					display: none;
				}
			}

			&.shift--alert {
				position: absolute;
				top: 35px;
			}

			&.has-scrolled {
				padding: 0;
				height: 60px;
				color: $c-header-text-scroll;
				background-color: $c-header-bg-scroll;

				.nav-standard {
					color: $c-header-text-scroll;

					.menu-item--meganav {
						padding-bottom: 24px;
				    	margin-bottom: -20px;
					}

					.megamenu-wrapper {
						padding-top: 70px;
					}
				}

				.container {
					top: 0;
					padding-top: 0;
				}

				.main-logo { margin-top: 0; }

				.main-logo__text,
				#cartTotal .cartToggle-items,
				.currency-picker { color: $c-header-text-scroll; }

				.main-logo,
				.header-fix-cont {
					height: 60px;
				}

				.header-icons svg path { fill: $c-header-text-scroll; }
			}

			&.header--logo_center_links_left,
			&.header--logo_left_links_left {
				.nav-standard {
					float: left;
					padding-top: 0;
					margin-left: -20px;
				}

				.main-logo {
					position: absolute;
					top: 0;
					left: 50%;
					margin-top: 10px;
					margin-bottom: 10px;

					@media screen and (min-width: $med) {
						transform: translateX(-50%);
					}
				}

				&.has-scrolled {
					.main-logo {
						margin-top: 10px;
						height: 40px;
					}

					.nav-standard {
						padding-top: 0;
					}
				}
			}

			&.header--logo_left_links_right,
			&.header--logo_center_links_right {
				.main-logo {
					float: left;
					white-space: nowrap;
					text-align: left;
					margin-top: 10px;
					margin-bottom: 10px;
				}

				.nav-standard {
					float: right;
					padding-top: 0;

					.menu { margin-right: 15px; }

					// If last item has Dropdown then show on left side
					.menu-item {
						&:last-child {
							& > .submenu {
								left: auto;
								right: 0;
							}

							.submenu {
								&::before {
									left: auto;
									right: 18px;
								}
							}

							.submenu-item > .submenu {
								left: auto;
								right: 100%;
							}
						}
					}
				}

				&.has-scrolled {
					.main-logo {
						margin-top: 10px;
						height: 40px;
					}

					.nav-standard {
						padding-top: 0;
					}
				}
			}

			&.header--logo_left_links_center {
				.main-logo {
					float: left;
					white-space: nowrap;
					text-align: left;
					margin-top: 10px;
					margin-bottom: 10px;
				}

				.nav-standard {
					position: absolute;
					top: 0;
					left: 0;
					right: 0;
					padding-left: 400px;
					padding-right: 400px;
					justify-content: center;
					margin-top: 0;

					.menu {
						width: auto;
						margin: 0;
					}

					.menu-item:last-child {
						margin-right: 0;
					}

				}

				&.has-scrolled {
					.main-logo {
						margin-top: 10px;
						height: 40px;
					}

					.nav-standard {
						top: 0;

						.menu {
							margin-top: 0;
						}
					}
				}
			}

			&.header--logo_center_links_center {
				height: 140px;
				.main-logo {
					position: absolute;
					top: 0;
					left: 50%;
					text-align: center;
					white-space: nowrap;
					visibility: visible;
					justify-content: center;
					margin-top: 10px;
					margin-left: auto;
					margin-right: auto;
					padding: 10px 0;
					height: 80px;
					@include transform(unquote('translateX(-50%)'));
					@include transition(unquote('margin .2s ease'));
				}

				.header-fix-cont {
					height: 140px;
				}

				&.main-logo__text {
					margin-top: 20px;
				}

				.nav-standard {
					position: static;
					margin-top: 0;
					padding-top: 100px;
					clear: both;
					height: auto;
					justify-content: center;

					.menu {
						width: auto;
						margin: 0;
					}

					.menu-item:last-child {
						margin-right: 0;
					}
				}

				.header-icons {
					position: absolute;
					top: 0;
					right: 0;
					max-height: 100px;
				}

				.megamenu-wrapper {
					padding-top: 140px;
				}

				.nav-search-overlay {
					min-height: 140px;
				}

				&.header--no-logo {
					.nav-standard {
						padding-top: 10px;
					}
				}

				&.has-scrolled {
					height: 60px;

					.header-fix-cont {
						height: 60px;
					}

					.main-logo {
						position: absolute;
						top: 0;
						left: 0;
						height: 40px;
						padding: 0;
						margin-top: 10px;
						text-align: left;
						justify-content: flex-start;
						@include transform(unquote('none'));
					}

					.nav-standard {
						padding-top: 0;
						height: 60px;
					}
				}
			}
		}
	}

	.container {
		text-align: center;
	}


	& + section:not(.hero) {
		padding-top: 120px;
		@media screen and (min-width: $med){
			padding-top: 160px;
		}
	}

	&.is-hamburger {
		position: absolute;
		top: auto;
		left: 0;
		width: 100%;

		&.has-scrolled {
			position: fixed;
			top: 0;
			color: $c-header-text-scroll;
			background: $c-header-bg-scroll;
			height: 60px;
			padding: 0;

			.container { top: 0; }

			.main-logo {
				margin-top: 10px;
				height: 40px;

				@media (max-width: ($med - 1)) {
					height: 60px;
					margin-top: 0;
				}
			}

			.main-logo__text {
				& + .main-logo {
					display: block;
				}

				@media (max-width: $sm) {
					display: none;
				}
			}

			.main-logo--text-only {
				.main-logo__text {
					@media (max-width: $sm) {
						display: block;
					}
				}
			}

			.header-fix-cont { height: 60px; }

			.main-logo__text,
			#cartTotal .cartToggle-items { color: $c-header-text-scroll; }

			.header-icons svg path { fill: $c-header-text-scroll; }
		}

		.header-fix-cont { height: 100px; }

		&.shift--alert {
			position: absolute;
			top: auto;
		}
	}

	@media screen and (max-width: ($med - 1)) {
		overflow: hidden;
	}

	@media screen and (max-width: ($sm - 1)) {
		padding: 0;
	}
}

.header-fix-cont {
	@include blockAb;
	@include transition(unquote('height .2s'));
	top: 0;
	left: 0;
	right: 0;
	z-index: 2;
}

.header-fix-cont-inner { height: 100%; }

/**
 * The "X" that closes the nav on mobile.
 */
.nav-mobile-close {
	position: absolute;
	right: 35px;
	top: 30px;
	display: flex;
	align-items: center;
	justify-content: center;
	width: 45px;
	height: 45px;
	z-index: 2;

	svg {
		display: block;
		width: 23px;
		height: 23px;
		path { fill: $c-drawer-text; }
	}

	@media screen and (min-width: ($med)) {
		display: none;
	}
}

/*
* Navigations
*/
.nav-hamburger {
	@include blockFix;
	bottom: 0;
	height: 100%;
	right: 100%;
	top: 0;
	width: 100%;
	z-index: 1000;
	-webkit-transform: translateZ(0);
	@include transition(unquote('transform .3s'));

	@media screen and (min-width: $sm){
		width: 100%;
	}
	@media screen and (min-width: $med){
		width: 33%;
	}
	@media screen and (min-width: $xl){
		width: 33%;
	}

	.nav--is-visible:not(.nav--is-hiding) & {
		@include transform(unquote('translateX(100%)'));
	}

	.menu {
		padding-bottom: 0;
		padding-right: 0;
		margin-bottom: 20px;
		overflow: hidden;
		text-align: left;
		min-height: 1px;
		z-index: 1;
		transition: height .2s ease;
	}

	.submenu {
		position: static;
		padding: 0 0 0 25px;
		opacity: 1;
		display: none;
	}

	.is-expanded > .submenu {
		pointer-events: all;
	}

	.menu-item,
	.submenu-item {
		position: relative;
		font-size: $font_bodytext_size * 1.29;
		line-height: 25px;
		margin: 7px 0 24px;

		&:last-child { margin-bottom: 0; }

		a {
			padding: 5px 0;
			line-height: inherit;

			// Prevent long link titles overlap with the dropdown arrow
			word-break: break-word;
		}

		.dropdown-arrow {
			position: absolute;
			top: 0;
			right: -4px;
			margin: 0;
			padding: 10px;
			width: 36px;
			height: 36px;
			display: flex;
			align-items: center;
			justify-content: center;
			cursor: pointer;
			color: $c-drawer-text;
			border: 0;
			background-color: transparent;
			appearance: none;
			-webkit-appearance: none;

			&::after {
				@include icon;
				@include transform(unquote('rotate(90deg)'));
				content: "\e601";
				font-size: 1rem;
				line-height: 16px;
			}
		}

		// Prevent arrow down overlapping
		&.has-submenu > a { padding-right: 24px; }
		&.has-submenu > a::after { content: none; }
		&.has-submenu.is-expanded > .dropdown-arrow::after {
			@include transform(unquote('rotate(270deg)'));
		}
	}

	.submenu-item {
		font-size: $font_bodytext_size * 1.21;
		margin: 0;

		a {
			border-bottom: 1px solid $c-border;
			padding: 12px 0;
			line-height: 20px;
		}

		.dropdown-arrow { top: 5px; }

		.submenu {
			border: 1px solid $c-border;
			border-top: 0;
			padding: 0 12px 0 22px;
			margin-bottom: 10px;

			.submenu-item {
				font-size: $font_bodytext_size * 1.07;

				&:last-child a { border-bottom: none; }
			}
		}
	}

	.currency-picker {
		font-size: inherit;
		line-height: inherit;
		padding-left: 0;
		color: $c-drawer-text;
	}
}

.nav-inner {
	@include blockAb;
	top: 0;
	left: 0;
	background: inherit;
	height: 100%;
	overflow-x: hidden;
	overflow-y: auto;
	width: 100%;
	padding: 0 45px 20px;
	-webkit-overflow-scrolling: touch;
	background-color: $c-drawer-bg;
	h4, i, a {color: $c-drawer-text;}

	& > nav.menu {
		padding-top: 74px;
	}

	.nav-social .nav-social-link::before,
	.nav-search .h4 {color: $c-drawer-text;}
}

.nav-user {
	border-top: 1px solid $c-drawer-text;
	padding: 33px 0 15px;
	font-size: $font_bodytext_size * 1.14;
	line-height: 25px;
}
.nav-user__item {
	margin-bottom: 17px;

	&:last-child { margin-bottom: 0; }
}
.nav-user__link {
	color: $c-dropdown-text;
}

/**
 * Navigation Ajax search
 */
.nav-search {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	margin: 0;

	.search-form {
		position: relative;
		width: 100%;
		margin: 34px auto 30px;
	}

	label.icon-search {
		position: absolute;
		top: 5px;
		left: 0;
		width: 22px;
		height: 22px;
		margin-bottom: 0;

		svg {
			display: block;
			width: 100%;
			height: 100%;
			path {
				fill: $c-dropdown-text;
			}
		}
	}

	.inputGroup {
		margin: 0 auto;
		width: 100%;
		height: 35px;
		border-bottom: 2px solid $c-dropdown-text;
	}
}

.nav-search-overlay {
	@include blockAb;
	@include transition(unquote('padding .2s ease, opacity .2s ease, visibility 0s linear .2s'));
	top: 0;
	left: -80px;
	right: -80px;
	opacity: 0;
	overflow: hidden;
	pointer-events: none;
	background-color: $c-dropdown-bg;
	box-shadow: 0 2px 5px rgba(0, 0, 0 ,.1);

	@media screen and (max-width: ($lg - 1)) {
		left: -40px;
		right: -40px;
	}

	@media screen and (max-width: ($sm - 1)) {
		left: -20px;
		right: -20px;
	}
}

.nav-search-input {
	font-size: $font_bodytext_size * 1.57;
	line-height: normal;
	height: 100%;
	padding: 0 47px;
	width: 100%;
	border-radius: 0;
	background: transparent;
	-webkit-appearance: none;
	box-sizing: border-box;
	color: $c-dropdown-text;
	box-sizing: border-box;
	box-shadow: none;

	&::placeholder { color: $c-dropdown-text; }
	&::-webkit-input-placeholder { color: $c-dropdown-text; }
	&:-moz-placeholder { color: $c-dropdown-text; }
	&::-moz-placeholder { color: $c-dropdown-text; }
	&:-ms-input-placeholder { color: $c-dropdown-text; }
}

.nav-search-submit {
	display: none;
}

.nav-search-scroller {
	max-height: 100vh;
	overflow-x: hidden;
	overflow-y: scroll;
}

.nav-search-container {
	position: relative;

	&.is-searching {
		.results::before {
			content: '';
			display: block;
			width: 79px;
			height: 64px;
			background: url({{'load-more.gif' | asset_url }}) no-repeat;
			background-size: contain;
			margin: 15px auto;
		}
	}
}

.search--is-visible {
	z-index: 5000;
	.nav-search-overlay {
		opacity: 1;
		visibility: visible;
		z-index: 20;
		pointer-events: auto;
		@include transition(unquote('opacity .4s ease, visibility 0s linear 0s'));
	}
}

.search--is-hiding {
	.nav-search-overlay {
		opacity: 0;
		visibility: hidden;
		@include transition(unquote('opacity .2s ease, visibility 0s linear .2s'));
	}
}

/*
 * Suggested results
 */
.suggested-results {
	display: flex;
	justify-content: space-between;
	padding: 12px 0 20px;
	margin-bottom: 20px;
	color: $c-dropdown-text;
}

.suggested-result {
	width: calc(50% - 21px);
	padding: 0;
	text-align: left;

	.suggested-results--3 & {
		width: 310px;
		max-width: calc(33.3333% - 21px);
	}
}

.suggested-result__title {
	font-size: $font_bodytext_size * 1.14;
	font-family: $general_font;
	font-weight: $general_font_weight;
	font-style: $general_font_style;
	color: inherit;
	border-bottom: 1px solid darken($c-border, 35%);
	padding-bottom: 4px;
	margin-bottom: 13px;
}

.suggested-result__collection-image {
	position: relative;
	overflow: hidden;
	display: flex;
	align-items: flex-end;
	width: 100%;
	height: 174px;
	background-size: cover;
	background-repeat: no-repeat;
	background-position: center center;
	color: {{ settings.search_suggested_text_color | default: '#fff' }};
	&::before {
		content: '';
		position: absolute;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		background-color: {{ settings.search_suggested_overlay_color | default: '#000' }};
		opacity: {{ settings.search_suggested_overlay_opacity | times: 0.01 | round: 2 }};
	}

	.image-overlay {
		position: relative;
		padding: 30px;
		width: 100%;
	}

	.collection-title {
		font-size: $font_base_size * 2.1;
		line-height: 1.2;
		margin-bottom: 10px;
		color: inherit;

		@media screen and (max-width: ($med - 1)){
			font-size: $font_base_size * 1.66;
		}
	}

	.collection-description {
		font-size: $font_bodytext_size * 1.11;
	}

	.collection-link {
		display: inline-block;
		padding-bottom: 3px;
		border-bottom: 1px solid {{ settings.search_suggested_text_color | default: '#fff' }};
	}
}


.suggested-result__item {
	display: block;
	color: inherit;
	margin-bottom: 22px;

	h4 {
		font-size: $font_bodytext_size * 1.14;
		line-height: 1.2;
		font-family: $general_font;
		font-weight: $general_font_weight;
		font-style: $general_font_style;
		margin-bottom: 6px;
		color: inherit;
	}

	p {
		color: inherit;
		margin: 0;
		line-height: 1.2;
	}
}

/*
 * Menu
 */

.menu {
	@include blockRel;
	min-height: 48%;
	text-align: center;
	width: 100%;
	padding-bottom: 35px;

	&.menu--has-meganav {
		position: static;
	}
}

.menu-item,
.submenu-item {
	/**
	 * Just the link is what get's animated out of frame.
	 */
	& > a {
		@include transition(unquote('transform .3s ease-in-out, opacity .3s, color .2s'));
		display: block;
		opacity: 1;
		line-height: 25px;
	}
}
.submenu-item > a {
	line-height: 20px;
}
.menu-item,
.submenu-item {
	font-size: $font_bodytext_size * 1.11;
	line-height: 1.5;
	margin: .6em auto;
	&.has-submenu > a::after {
		@include blockAb;
		@include icon;
		content: "\e601";
		font-size: 0.85rem;
		right: -0.5em;
		top: 0;
	}
}
.menu-divider {
	@include inlineRel;
	@include transition(unquote('transform .3s ease-in-out, opacity .3s'));
	height: 1px;
	margin: 25px 0;
	width: 20px;
}

/**
 * Submenu
 */
.submenu {
	@include blockAb;
	@include transition(unquote('transform .3s ease-in-out, opacity .3s'));
	left: 0;
	opacity: 0;
	right: 0;
	top: 0;
	z-index: 9999;
	background: {{ settings.drawer_bg_color }};
	pointer-events: none;
	.submenu--is-visible & {
		/**
		 * Only those .submenu's with this class animate in.
		 */
		&.is-visible {
			opacity: 1;
			pointer-events: auto;
		}
	}
}
.submenu-item {
	font-size: $font_bodytext_size * 1.11;
	line-height: 1.5;
	margin-bottom: 10px;
}
.submenu-back {
	margin-bottom: 20px;
	opacity: 0.7;
	@include transition(opacity 150ms);
	&:hover { opacity: 1; }
	a {
		display: inline-block;
		width: 90%;
	}
	a:before {
		@include blockAb;
		@include icon;
		content: "\e600";
		font-size: 0.85rem;
		left: -0.5em;
		top: 0;
	}
}

.nav-standard {
	display: none;
	align-items: center;
	justify-content: flex-start;
	height: 120px;
	min-height: 90px;
	margin-top: 0;
	@include transition(unquote('padding .2s ease, top .2s ease, height .2s ease'));

	.has-scrolled & {
		height: 60px;
		min-height: 60px;
	}

	.menu {
		display: flex;
		align-items: flex-start;
		min-height: 1px;
		padding: 0;
		height: auto !important;
	}

	.menu-item,
	.submenu-item {
		position: relative;
		font-family: $general_font;
		font-size: 14px;

		& > a {
			display: block;
			width: auto;
			white-space: nowrap;
		}

		&.has-submenu > a {
			position: relative;
			display: inline-block;

			&::after {
				font-size: 0.85rem;
				right: -17px;
				top: 50%;
				position: absolute;
				transform: translateY(-50%);
				margin-top: 2px;
			}
		}

		&:hover > .submenu {
			opacity: 1;
			visibility: visible;
			margin-top: 0;
			@include transition(unquote('margin .3s ease, opacity .3s ease, visibility 0s linear 0s'));
		}
	}

	.menu-item {
		margin: 0 9px 0 0;
		padding: 5px 20px;

		&.has-submenu > a {

			&::after {
				content: '\e902';
			}
		}
	}

	.submenu {
		position: absolute;
		top: 100%;
		left: 0;
		right: auto;
		margin-top: 8px;
		z-index: 1000;
		background: $c-dropdown-bg;
		color: $c-dropdown-text;
		border-radius: 3px;
		width: auto;
		min-width: 124px;
		text-align: left;
		padding: 4px 0;
		pointer-events: all;
		opacity: 0;
		visibility: hidden;
		box-shadow: 0 2px 5px rgba(0, 0, 0 ,.1);
		@include transform(unquote('none'));
		@include transition(unquote('margin .3s ease .1s, opacity .3s ease .1s, visibility 0s linear .4s'));

		&::before {
			content: '';
			position: absolute;
			bottom: 100%;
			left: 18px;
			border-style: solid;
			border-width: 0 4px 4px;
			border-color: transparent transparent $c-dropdown-bg;
		}

		.submenu-item {
			display: block;
			margin: 0 0 4px;

			&.has-submenu {
				position: relative;

				& > a::after {
					content: '\e601';
					@include transform(unquote('none'));
					display: inline-block;
					vertical-align: middle;
					margin: 7px 0 0 4px;
					top: auto;
					right: auto;
				}
			}

			a {
				display: block;
				color: $c-dropdown-text;
				width: auto;
				padding: 5px 22px;

				@include transition(unquote('opacity .2s ease, transform .2s ease'));
			}

			&:hover > a {
				@include transform(unquote('translateX(2px)'));
				opacity: .7;
			}

			.submenu {
				top: 0;
				left: 100%;
				right: auto;
				margin: 0 1px;

				&::before {
					content: none;
				}
			}
		}
	}

	.login-links {
		padding: 8px 0;
		position: static;
	}

	.nav-footer {
		padding: 0;
	}

	// Mega Nav
	.menu-item--meganav {
		position: static;

    	&:hover,
    	&.is-expanded {
    		padding-bottom: 57px;
    		margin-bottom: -53px;
    	}

		&:hover > .megamenu-wrapper,
		&.is-expanded > .megamenu-wrapper {
			opacity: 1;
			visibility: visible;
			@include transition(unquote('padding .2s ease, opacity .3s ease, visibility 0s linear 0s'));

			.submenu {
				pointer-events: auto;
			}
		}

		.megamenu__image {
			float: left;
			width: 29%;
			margin-right: 6%;
			text-align: left;
			padding-bottom: 20px;

			img {
				display: block;
				width: 100%;
				height: auto;
				margin-bottom: 20px;

				&.lazyload,
				&.lazyloading {
					min-height: 100px;
				}
			}

			a {
				width: 100%;
				opacity: 1;
			}

			h2 {
				font-size: $font_size_h1;
			}

			h4 {
				text-transform: uppercase;
				letter-spacing: 1px;
				font-size: $font_base_size;
				font-family: $general_font;
				font-weight: $general_font_weight;
				position: relative;
				display: flex;

				span {
					position: relative;
					overflow: hidden;
					text-overflow: ellipsis;
					white-space: nowrap;
					background: $c-dropdown-bg;
					padding-right: 16px;
					max-width: 90%;
					font-family: $general_font;
					font-weight: $general_font_weight;

				}

				&::before {
					content: '';
					position: absolute;
					top: 50%;
					left: 0;
					width: 100%;
					height: 1px;
					background: $c-border;
				}
			}
		}

		.submenu-item {
			position: static;

			&.has-submenu > a::after {
				content: none;
			}
		}

		.submenu {
			position: static;
			pointer-events: none;
			opacity: 1;
			visibility: visible;
			box-shadow: none;
			padding: 0;
			margin: 0;
			background: transparent;
			overflow: hidden;
			display: flex;
			flex-wrap: wrap;
			@include transition(unquote('none'));
			@include transform(unquote('none'));

			&::before {
				content: none;
			}

			.submenu-item {
				width: 25%;
				padding-bottom: 20px;
				padding-right: 40px;

				&:nth-child(4n+1) {
					clear: left;
				}

				a {
					display: inline-block;
					opacity: 1;
					padding: 0;
					white-space: normal;

					&.submenu-item__title {
						padding: 5px 0;
						font-size: $font_base_size * 1.33;
						font-weight: $font_weight_medium;
						margin-bottom: 10px;
						opacity: 1;
					}
				}

				&:hover > a {
					@include transform(unquote('none'));
				}

				.submenu {
					top: auto;
					left: auto;
					margin: 0;
					display: block;

					.submenu-item {
						float: none;
						display: block;
						padding-bottom: 0;
						margin-bottom: 5px;
						width: 100%;
					}
				}
			}

			&.submenu--items-1 {
				.submenu-item {
					width: 90%;
				}
			}
			&.submenu--items-2 {
				.submenu-item {
					width: 40%;

					&:nth-child(2n+1) {
						clear: left;
					}
				}
			}

			&.submenu--items-3 {
				.submenu-item {
					width: 33.33%;

					&:nth-child(4n+1) {
						clear: none;
					}

					&:nth-child(3n+1) {
						clear: left;
					}
				}
			}
		}
	}
}

.megamenu-wrapper {
	opacity: 0;
	visibility: hidden;
	margin-top: 0px;
	z-index: -1;
	box-shadow: 0 2px 5px rgba(0, 0, 0 ,.1);
	position: absolute;
	top: 0;
	left: -80px;
	right: -80px;
	padding-top: 130px;
	background: $c-dropdown-bg;
	color: $c-dropdown-text;
	pointer-events: auto;
	@include transition(unquote('padding .2s, opacity .1s ease, visibility 0s linear .1s'));

	@media screen and (max-width: ($lg - 1)) {
		left: -40px;
		right: -40px;
	}

	@media screen and (max-width: ($sm - 1)) {
		left: -20px;
		right: -20px;
	}
}

.megamenu-inner {
	padding-top: 24px;
	max-height: calc(100vh - 60px);
	overflow-y: auto;
	overflow-x: hidden;
	@include transition(unquote('margin .2s'));
}

.nav-social {
	margin: 10px -4px;
	padding: 0;
}
.nav-social-link {
	@include inlineRel;
	font-size: 1.3rem;
	line-height: 32px;
	margin: 0 12px 0 0;
	text-align: center;
	width: 36px;
	height: 36px;
	padding: 4px;
	.diamond {
		opacity: 0.5;
	}
}

.nav-signUp {
	padding: 30px;
	text-align: center;
}
.nav-signUp-form {
	@include blockRel;
	margin: 15px auto 0;
	width: 280px;
	@media screen and (min-width: $med){
		width: 360px;
	}
	&.success-response--is-visible, &.error-response--is-visible {
		.inputGroup {
			opacity: 0;
		}
	}
	.inputGroup {
		@include blockRel;
		height: 35px;
		width: 100%;
	}
}

.header-icons {
	float: right;
	margin-right: -5px;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: flex-end;

	.nav-item {
		width: 32px;
		height: 32px;
		display: flex;
		align-items: center;
		justify-content: center;
		margin-right: 16px;
		cursor: pointer;
		position: relative;
		z-index: 2;

		@media screen and (max-width: ($med - 1)) {
			margin-right: 8px;
		}

		&:last-child { margin-right: 0; }
	}

	.nav-item--currency-picker {
		width: auto;

		.currency-picker { font-size: inherit; }

		select.currency-picker,
		.currency-picker .selectArrow {
			color: inherit;
			@include transition(unquote('color .2s'));
		}

		.is-hamburger & {
			display: none;
		}
	}

	svg path {
		fill: $c-header-text;
		@include transition(unquote('all .2s'));
	}

	.searchToggle { top: auto; }

	.icon-search {
		display: block;
		width: 24px;
		height: 24px;
	}
}

.nav-account-link {
	position: relative;
	z-index: 2;
    width: 30px;
    height: 30px;
    display: flex;
    align-items: center;
    justify-content: center;
    @include transition(unquote('top .2s'));

	svg {
		width: 25px;
		height: 25px;
		display: block;
	}

	@media screen and (max-width: $xs) {
		margin-right: 10px;
	}
}

.header--no-bg {
	.main-logo__text,
	.cartToggle .cartToggle-items { color: $c-header-text-home; }

	.header-icons svg path { fill: $c-header-text-home; }

	.menuToggle .diamond-bar { background-color: $c-header-icon-bg-home; }

	.icon-menu .icon-bar,
	.icon-menu .icon-bar:before,
	.icon-menu .icon-bar:after{
		background-color: $c-header-icon-home;
	}


	&.has-scrolled {
		.menuToggle .diamond-bar { background-color: $c-header-icon-bg-scroll; }

		.icon-menu .icon-bar,
		.icon-menu .icon-bar:before,
		.icon-menu .icon-bar:after{
			background-color: $c-header-icon-scroll;
		}
	}
}

/*
 * Login Items
 */
.login-links {
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	text-align: center;
	font-family: $header_font;
}

.ripple-click {
	-webkit-animation: anim-effect-jelena 0.3s ease-out forwards;
	animation: anim-effect-jelena 0.3s ease-out forwards;
}

@-webkit-keyframes anim-effect-jelena {
	0% {
		opacity: 1;
		-webkit-transform: scale3d(0.5, 0.5, 1) rotate(45deg);
		transform: scale3d(0.5, 0.5, 1) rotate(45deg);
	}
	to {
		opacity: 0;
		-webkit-transform: scale3d(1.2, 1.2, 1) rotate(45deg);
		transform: scale3d(1.2, 1.2, 1) rotate(45deg);
	}
}

@keyframes anim-effect-jelena {
	0% {
		opacity: 1;
		-webkit-transform: scale3d(0.5, 0.5, 1) rotate(45deg);
		transform: scale3d(0.5, 0.5, 1) rotate(45deg);
	}
	to {
		opacity: 0;
		-webkit-transform: scale3d(1.2, 1.2, 1) rotate(45deg);
		transform: scale3d(1.2, 1.2, 1) rotate(45deg);
	}
}

.mo-padding {
	margin-top: 60px;
}

@media screen and (max-width: ($lg - 1)) {
	.mo-padding {margin-top:50px;}
}

/**
 * Menu Icon
 */
.menuToggle {
	position: absolute;
	left: 0;
	top: 50%;
	margin-top: -24px;
	font-size: 20px;
	padding: 10px 10px 12px;
	z-index: 100;
	cursor: pointer;
	@include transition( unquote('opacity .1s, background .2s ease, left .3s, top .2s') );

	/**
	 * Square behind hamburger, rotates when menu is open.
	 */
	.diamond-bar {
		background-color: $c-header-icon-bg;

		.has-scrolled & {
			background-color: $c-header-icon-bg-scroll;
		}
	}
}

.icon-menu .icon-bar,
.icon-menu .icon-bar:before,
.icon-menu .icon-bar:after{
	background-color: $c-header-icon;

	.has-scrolled & {
		background-color: $c-header-icon-scroll;
	}
}

/**
 * Cart Icon
 */
.cartToggle {
	@include transition( unquote('background .2s ease, right .3s, top .2s') );
	color: $c-text;
	display: flex;
	align-items: center;
	justify-content: center;
	float: right;
	height: 30px;
	width: 30px;
	position: relative;
	z-index: 2;
	/**
	 * Show "CART" text.
	 */
	&:hover::after {
		opacity: 1;
		transform: translateY(0);
		-webkit-transform: translateY(0);
		-moz-transform: translateY(0);
		-ms-transform: translateY(0);
	}
	/**
	 * "CART" text.
	 */
	&::after {
		@include blockAb;
		@include transition( unquote('transform 0.1s, opacity .2s') );
		color: $c-text;
		left: -10px;
		opacity: 0;
		right: -10px;
		top: 35px;
		text-align: center;
		transform: translateY(-15px);
		-webkit-transform: translateY(-15px);
		-moz-transform: translateY(-15px);
		-ms-transform: translateY(-15px);
		.header--no-bg & {
			color: $c-text;
		}
	}

	/**
	 * If .hero is present, remove background.
	 */
	.header--no-bg & {
		background-color: rgba(white, 0);

		&.diamond-outline {
			&:hover .diamond {
				border-color: $c-text;
			}
			.diamond {
				background-color: transparent;
				border-color: $c-text;
			}
		}
	}

		/**
		 * Child of .diamond-outline this is the bit that's visible and has the hover effect.
		 */
	&.diamond-outline {
		&:hover .diamond {
			border-color: $c-text;
		}
		.diamond {
			background-color: $c-background;
			border-color: $c-text;
		}
	}

	/**
	 * The number of items within the cartToggle.
	 */
	.cartToggle-items {
		@include blockAb;
		right: 0;
		left: 0;
		bottom: -2px;
		font-size: 80%;
		color: $c-header-text;
		line-height: 100% !important;
		padding: 8px 0;
		text-align: center;
		letter-spacing: 0px !important;
		@include transition(unquote('color .2s'));
	}

	path {
		fill: $c-header-text;
		@include transition(unquote('all .2s'));
	}

	svg {
    	width: 27px;
    	height: 27px;
	}
}

.searchToggle {
	position: absolute;
	top: 2px;
	right: 0;
	z-index: 2;
	width: 30px;
	height: 30px;
	display: flex;
	align-items: center;
	justify-content: center;
	cursor: pointer;
	@include transition(unquote('top .2s'));

	.icon-close {
		width: auto;
		height: auto;
		padding: 0;
		line-height: normal;
		display: block;
		font-size: 3em;
		color: $c-dropdown-text;
	}

	path { fill: $c-drawer-text; }

	.is-hamburger & { display: none; }
}

/*
 * Main Logo
 */
.main-logo {
	display: flex;
	align-items: center;
	justify-content: center;
	height: 100px;
	position: relative;
	z-index: 3;
	margin: 0 auto;
	text-align: center;
	font-family: $general_font;
	@include transition(unquote('height .2s'));

	@media screen and (max-width: ($med - 1)) {
		flex-direction: column;
		margin-top: 0;
		padding: 10px 0;
	}

	.logo-home {
		position: absolute;
		visibility: hidden;
	}

	.is-hamburger & {
		max-width: calc(100% - 140px);
	}

	.template-index & {
		.logo-home {
			position: relative;
			visibility: visible;
		}
		.logo-other:not(:only-child) {
			position: absolute;
			visibility: hidden;
		}
	}
}

.main-logo__image {
	float: left;
	display: flex;
	align-items: center;
	justify-content: center;

	img {
		display: block;
		width: auto;
		max-width: 100%;
		max-height: 100px;
		object-fit: contain;
		@include transition(unquote('max-width .2s, max-height .2s'));

		@media (max-width: ($med - 1)) {
			max-height: 80px;
		}
	}

	.has-scrolled & {
		img { max-height: 40px; }
	}

	& + .main-logo__text {
		padding-left: 15px;

		@media (max-width: $sm){
			padding: 10px 0;
		}
	}

	@media (max-width: $sm) {
		max-width: 100% !important;
	}
}

.main-logo__text {
	font-size: $font_bodytext_size * 1.5;
	font-family: $general_font;
	font-style: $general_font_style;
	font-weight: $general_font_weight;
	line-height: 30px;
	padding: 0;
	margin-bottom: 0;
	color: $c-header-text;
	@include transition(unquote('color .2s'));

	&.inverted {
		color: invert($c-header-text);
	}

	@media (min-width: $med){
		white-space: nowrap;
	}

	@media (max-width: ($med - 1)) {
		display: none;

		.main-logo--text-only & {
			display: block;
		}
	}
}

.slick-prev,
.slick-next {
	background: none !important;
}

/**
 * Magnific popup
 */

/* overlay */
$mfp-overlay-color:                   #0b0b0b !default;                    /* Color of overlay screen */
$mfp-overlay-opacity:                 0.8 !default;                        /* Opacity of overlay screen */
$mfp-shadow:                          0 0 8px rgba(0, 0, 0, 0.6) !default; /* Shadow on image or iframe */

/* spacing */
$mfp-popup-padding-left:              8px !default;                        /* Padding from left and from right side */
$mfp-popup-padding-left-mobile:       6px !default;                        /* Same as above, but is applied when width of window is less than 800px */

$mfp-z-index-base:                    1040 !default;                       /* Base z-index of popup */

/* controls */
$mfp-include-arrows:                  true !default;                       /* Include styles for nav arrows */
$mfp-controls-opacity:                0.65 !default;                       /* Opacity of controls */
$mfp-controls-color:                  #FFF !default;                       /* Color of controls */
$mfp-controls-border-color:           #3F3F3F !default;                    /* Border color of controls */
$mfp-inner-close-icon-color:          #333 !default;                       /* Color of close button when inside */
$mfp-controls-text-color:             #CCC !default;                       /* Color of preloader and "1 of X" indicator */
$mfp-controls-text-color-hover:       #FFF !default;                       /* Hover color of preloader and "1 of X" indicator */

/* Iframe-type options */
$mfp-include-iframe-type:             true !default;                       /* Enable Iframe-type popups */
$mfp-iframe-padding-top:              40px !default;                       /* Iframe padding top */
$mfp-iframe-background:               #000 !default;                       /* Background color of iframes */
$mfp-iframe-max-width:                900px !default;                      /* Maximum width of iframes */
$mfp-iframe-ratio:                    9/16 !default;                       /* Ratio of iframe (9/16 = widescreen, 3/4 = standard, etc.) */

/* Image-type options */
$mfp-include-image-type:              true !default;                       /* Enable Image-type popups */
$mfp-image-background:                #444 !default;
$mfp-image-padding-top:               40px !default;                       /* Image padding top */
$mfp-image-padding-bottom:            40px !default;                       /* Image padding bottom */
$mfp-include-mobile-layout-for-image: true !default;                       /* Removes paddings from top and bottom */

/* Image caption options */
$mfp-caption-title-color:             #F3F3F3 !default;                    /* Caption title color */
$mfp-caption-subtitle-color:          #BDBDBD !default;                    /* Caption subtitle color */

/* A11y */
$mfp-use-visuallyhidden:              false !default;                      /* Hide content from browsers, but make it available for screen readers */

/* Transluscent overlay */
.mfp-bg {
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: $mfp-z-index-base + 2;
	overflow: hidden;
	position: fixed;

	background: $mfp-overlay-color;
	opacity: $mfp-overlay-opacity;
}

/* Wrapper for popup */
.mfp-wrap {
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: $mfp-z-index-base + 3;
	position: fixed;
	outline: none !important;
	-webkit-backface-visibility: hidden; /* fixes webkit bug that can cause "false" scrollbar */
}

/* Root container */
.mfp-container {
	text-align: center;
	position: absolute;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	padding: 0 $mfp-popup-padding-left;
	box-sizing: border-box;
}

/* Vertical centerer helper */
.mfp-container {
	&::before {
		content: '';
		display: inline-block;
		height: 100%;
		vertical-align: middle;
	}
}

/* Remove vertical centering when popup has class `mfp-align-top` */
.mfp-align-top {
	.mfp-container {
		&::before {
			display: none;
		}
	}
}

/* Popup content holder */
.mfp-content {
	position: relative;
	display: inline-block;
	vertical-align: middle;
	margin: 0 auto;
	text-align: left;
	z-index: $mfp-z-index-base + 5;
}
.mfp-inline-holder,
.mfp-ajax-holder {
	.mfp-content {
		width: 100%;
		cursor: auto;
	}
}

/* Cursors */
.mfp-ajax-cur {
	cursor: progress;
}
.mfp-zoom-out-cur {
	&, .mfp-image-holder .mfp-close {
		cursor: -moz-zoom-out;
		cursor: -webkit-zoom-out;
		cursor: zoom-out;
	}
}
.mfp-zoom {
	cursor: pointer;
	cursor: zoom-in;
	cursor: -moz-zoom-in;
	cursor: -webkit-zoom-in;
	cursor: url('./magnify.cur'), move;
}
.mfp-auto-cursor {
	.mfp-content {
		cursor: auto;
	}
}

.mfp-close,
.mfp-arrow,
.mfp-preloader,
.mfp-counter {
	-webkit-user-select:none;
	-moz-user-select: none;
	user-select: none;
}

/* Hide the image during the loading */
.mfp-loading {
	&.mfp-figure {
		display: none;
	}
}

/* Helper class that hides stuff */
@if $mfp-use-visuallyhidden {
	/* From HTML5 Boilerplate https://github.com/h5bp/html5-boilerplate/blob/v4.2.0/doc/css.md#visuallyhidden */
	.mfp-hide {
		border: 0 !important;
		clip: rect(0 0 0 0) !important;
		height: 1px !important;
		margin: -1px !important;
		overflow: hidden !important;
		padding: 0 !important;
		position: absolute !important;
		width: 1px !important;
	}
} @else {
	.mfp-hide {
		display: none !important;
	}
}


////////////////////////
/* 2. Appearance */
////////////////////////

/* Preloader and text that displays error messages */
.mfp-preloader {
	color: $mfp-controls-text-color;
	position: absolute;
	top: 50%;
	width: auto;
	text-align: center;
	margin-top: -0.8em;
	left: 8px;
	right: 8px;
	z-index: $mfp-z-index-base + 4;
	a {
		color: $mfp-controls-text-color;
		&:hover {
			color: $mfp-controls-text-color-hover;
		}
	}
}

/* Hide preloader when content successfully loaded */
.mfp-s-ready {
	.mfp-preloader {
		display: none;
	}
}

/* Hide content when it was not loaded */
.mfp-s-error {
	.mfp-content {
		display: none;
	}
}

/* CSS-reset for buttons */
button {
	&.mfp-close,
	&.mfp-arrow {
		overflow: visible;
		cursor: pointer;
		background: transparent !important;
		border: 0;
		-webkit-appearance: none;
		display: block;
		outline: none;
		padding: 0;
		z-index: $mfp-z-index-base + 6;
		box-shadow: none;
		touch-action: manipulation;
	}
	&::-moz-focus-inner {
			padding: 0;
			border: 0
	}
}


/* Close icon */
.mfp-close {
	width: 44px;
	height: 44px;
	line-height: 44px;

	position: absolute;
	right: 0;
	top: 0;
	text-decoration: none;
	text-align: center;
	opacity: $mfp-controls-opacity;
	padding: 0 0 18px 10px;
	color: $mfp-controls-color;

	font-style: normal;
	font-size: 40px;

	&:hover,
	&:focus {
		opacity: 1;
	}
}
.mfp-close-btn-in {
	.mfp-close {
		color: $mfp-inner-close-icon-color;
	}
}
.mfp-image-holder,
.mfp-iframe-holder {
	.mfp-close {
		color: $mfp-controls-color;
		right: -10px;
		text-align: right;
		padding-right: 0;
		width: auto;
	}
}

/* "1 of X" counter */
.mfp-counter {
	position: absolute;
	top: 0;
	right: 0;
	color: $mfp-controls-text-color;
	font-size: 12px;
	line-height: 18px;
	white-space: nowrap;
}

/* Navigation arrows */
@if $mfp-include-arrows {
	.mfp-arrow {
		position: absolute;
		opacity: $mfp-controls-opacity;
		margin: 0;
		top: 50%;
		margin-top: -55px;
		padding: 0;
		width: 90px;
		height: 110px;
		-webkit-tap-highlight-color: rgba(0,0,0,0);
		&:active {
			margin-top: -54px;
		}
		&:hover,
		&:focus {
			opacity: 1;
		}
		&::before,
		&::after {
			content: '';
			display: block;
			width: 0;
			height: 0;
			position: absolute;
			left: 0;
			top: 0;
			margin-top: 35px;
			margin-left: 35px;
			border: medium inset transparent;
		}

		&::after {

			border-top-width: 13px;
			border-bottom-width: 13px;
			top:8px;
		}

		&::before {
			border-top-width: 21px;
			border-bottom-width: 21px;
			opacity: 0.7;
		}

	}

	.mfp-arrow-left {
		left: 0;
		&::after {
			border-right: 17px solid $mfp-controls-color;
			margin-left: 31px;
		}
		&::before {
			margin-left: 25px;
			border-right: 27px solid $mfp-controls-border-color;
		}
	}

	.mfp-arrow-right {
		right: 0;
		&::after {
			border-left: 17px solid $mfp-controls-color;
			margin-left: 39px
		}
		&::before {
			border-left: 27px solid $mfp-controls-border-color;
		}
	}
}



/* Iframe content type */
@if $mfp-include-iframe-type {
	.mfp-iframe-holder {
		padding-top: $mfp-iframe-padding-top;
		padding-bottom: $mfp-iframe-padding-top;
		.mfp-content {
			line-height: 0;
			width: 100%;
			max-width: $mfp-iframe-max-width;
		}
		.mfp-close {
			top: -40px;
		}
	}
	.mfp-iframe-scaler {
		width: 100%;
		height: 0;
		overflow: hidden;
		padding-top: $mfp-iframe-ratio * 100%;
		box-shadow: $mfp-shadow;
		background: $mfp-iframe-background;

		iframe {
			position: absolute;
			display: block;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
		}
	}
}



/* Image content type */
@if $mfp-include-image-type {

	/* Main image in popup */
	img {
		&.mfp-img {
			width: auto;
			max-width: 100%;
			height: auto;
			display: block;
			line-height: 0;
			box-sizing: border-box;
			padding: $mfp-image-padding-top 0 $mfp-image-padding-bottom;
			margin: 0 auto;
			cursor: default;
		}
	}

	/* The shadow behind the image */
	.mfp-figure {
		line-height: 0;
		&::after {
			content: '';
			position: absolute;
			left: 0;
			top: $mfp-image-padding-top;
			bottom: $mfp-image-padding-bottom;
			display: block;
			right: 0;
			width: auto;
			height: auto;
			z-index: -1;
			box-shadow: $mfp-shadow;
			background: $mfp-image-background;
		}
		small {
			color: $mfp-caption-subtitle-color;
			display: block;
			font-size: 12px;
			line-height: 14px;
		}
		figure {
			margin: 0;
		}
	}
	.mfp-bottom-bar {
		margin-top: -$mfp-image-padding-bottom + 4;
		position: absolute;
		top: 100%;
		left: 0;
		width: 100%;
		cursor: auto;
	}
	.mfp-title {
		text-align: left;
		line-height: 18px;
		color: $mfp-caption-title-color;
		word-wrap: break-word;
		padding-right: 36px; /* leave some space for counter at right side */
	}

	.mfp-image-holder {
		.mfp-content {
			max-width: 100%;
		}
	}

	.mfp-gallery {
		.mfp-image-holder {
			.mfp-figure {
				cursor: pointer;
			}
		}

		.mfp-arrow {
			font-size: 1.8rem;
			display: flex;
			align-items: center;
			justify-content: center;

			&::before {
				border: 0;
				margin: 0;
				position: static;
				width: auto;
				height: auto;
			}
			&::after {
				content: none;
			}

			&.icon-arrow-left:before {
				content: "\e600";
			}

			&.icon-arrow-right:before {
				content: "\e601";
			}
		}
	}


	@if $mfp-include-mobile-layout-for-image {
		@media screen and (max-width: 800px) and (orientation:landscape), screen and (max-height: 300px) {
			/**
			 * Remove all paddings around the image on small screen
			 */
			.mfp-img-mobile {
				.mfp-image-holder {
					padding-left: 0;
					padding-right: 0;
				}
				img {
					&.mfp-img {
						padding: 0;
					}
				}
				.mfp-figure {
					/* The shadow behind the image */
					&::after {
						top: 0;
						bottom: 0;
					}
					small {
						display: inline;
						margin-left: 5px;
					}
				}
				.mfp-bottom-bar {
					background: rgba(0,0,0,0.6);
					bottom: 0;
					margin: 0;
					top: auto;
					padding: 3px 5px;
					position: fixed;
					box-sizing: border-box;
					&:empty {
						padding: 0;
					}
				}
				.mfp-counter {
					right: 5px;
					top: 3px;
				}
				.mfp-close {
					top: -3px;
					right: 0;
					width: 35px;
					height: 35px;
					line-height: 35px;
					background: rgba(0, 0, 0, 0.6);
					position: fixed;
					text-align: center;
					padding: 0;
				}
			}
		}
	}
}

/* Scale navigation arrows and reduce padding from sides */
@media all and (max-width: 900px) {
	.mfp-arrow {
		-webkit-transform: scale(0.75);
		transform: scale(0.75);
	}
	.mfp-arrow-left {
		-webkit-transform-origin: 0;
		transform-origin: 0;
	}
	.mfp-arrow-right {
		-webkit-transform-origin: 100%;
		transform-origin: 100%;
	}
	.mfp-container {
		padding-left: $mfp-popup-padding-left-mobile;
		padding-right: $mfp-popup-padding-left-mobile;
	}
}


