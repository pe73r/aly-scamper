/*
 * Page Banners - Collections Page and About Page
 */
.banner {
	display: block;

	.banner-inner {
		@include blockRel;
		height: 440px;

		// Banner overlay
		&::after {
			content: '';
			position: absolute;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
		}

		&.banner-padded {
			@media screen and (min-width: $lg) {
				height: 750px;
				padding-top: 0;
			}
		}

		&.banner-padded--normal {
			@media screen and (min-width: $lg) {
				height: 650px;
				padding-top: 0;
			}
		}

		&.banner-padded--small {
			@media screen and (min-width: $lg) {
				height: 550px;
				padding-top: 0;
			}
		}

		&.banner-padded--one-hundred {
			height: 100vh;
			padding-top: 0;
		}

		&.banner-padded--original-height {
			height: auto;
			padding-top: 0;

			.banner-img--original {
				display: block;
				width: 100%;
				height: auto;
				object-fit: cover;
				transition: opacity 200ms ease-in-out, min-height .3s ease !important;
			}

			.banner__title {
				position: absolute;
				top: 0;
				right: 0;
				bottom: 0;
				left: 0;
				display: flex;
				align-items: center;
				justify-content: center;
			}
		}

		.banner-img {
			@include blockAb;

			bottom: 0; left: 0; right: 0; top: 0;
			background-size: cover;
			background-position: center;
			background-repeat: no-repeat;
		}
	}

	.page-title {
		position: absolute;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		z-index: 1;
		display: flex;
		align-items: center;
		width: 100%;
		height: 100%;
		margin: 0;
		text-align: inherit;

		.container {
			position: relative;
		}
	}

	.banner--text-center {
		text-align: center;
	}

	.banner--text-left,
	.banner--text-bottom-left {
		text-align: left;
	}

	.banner--text-right,
	.banner--text-bottom-right {
		text-align: right;
	}

	.banner--text-bottom-left,
	.banner--text-bottom-right {
		.page-title {
			align-items: flex-end;
			padding-bottom: 40px;

			@media screen and (max-width: $sm){
				padding-bottom: 20px;
			}
		}
	}
}

