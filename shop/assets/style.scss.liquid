// Variant
#VariantPopoverContainer {
	// when product images are in slideshow, hide popover
	display: none;
	text-align: center;
	@media (min-width: $med){
		display: block;
	}
	&:hover {
		.icon-arrow-left {
			color: rgba($c-text, 1);
		}
	}
	.popover {
		@include transition(unquote('opacity 0.5s cubic-bezier(.71,.01,.27,1), transform 0.5s cubic-bezier(.71,.01,.27,1)'));
		display: inline-block;
		opacity: 1;
		margin-top: 20px;
		min-width: 160px;
		opacity: 0;
		pointer-events: none;
		transform: translateY(calc(75px));
		-webkit-transform: translateY(calc(75px));
		-moz-transform: translateY(calc(75px));
		-ms-transform: translateY(calc(75px));

		&.is-visible {
			opacity: 1;
			pointer-events: auto;
			transform: translateY(0);
			-webkit-transform: translateY(0);
			-moz-transform: translateY(0);
			-ms-transform: translateY(0);
		}

		.popover-inner {
			padding: 20px 20px 10px;
			width: 100%;
			height: 100%;

			.popover-item-thumb {
				width: 100%;
				height: 90px;
				float: none;
			}
			h4 {
				padding: 10px 0 3px;
				text-align: center;
			}
		}
	}

	// little checkmark circle
	.product-selected-mark {
		top: 10px; left: 10px;
		pointer-events: none;
	}
