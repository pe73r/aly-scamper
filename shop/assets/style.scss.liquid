/*
 * Sticky "Shop" Bar
 */
.shopBar {
	@include blockFix;
	@include transition(unquote('transform .2s ease-in-out, left .3s'));

	background-color: white;
	background-color: $c-background;
	bottom: 0; left: 0; right: 0;
	box-shadow: 0 -2px 3px rgba(darken($c-background, 3%), .7);
	color: $c-text;
	padding: 15px 0 5px;
	transform: translateY(100%);
		-webkit-transform: translateY(100%);
		-moz-transform: translateY(100%);
		-ms-transform: translateY(100%);

	@media (max-width: ($med - 1)) {
		display: none;
	}

	// So social media icons don't break, hides social-block and floats button to the right
	@media (max-width: 970px) {
		.title-block {
			width:calc(80% - 20px/2.666666 ) !important;
		}
		.button-block {
			text-align: right !important;
		}
		.social-block {
			display: none !important;
		}
	}

	&.is-visible {
		transform: translateY(0);
		-webkit-transform: translateY(0);
		-moz-transform: translateY(0);
		-ms-transform: translateY(0);
	}

	.to-top {
		border-left: 1px solid $c-border;
		color: $c-text;
		padding: 0 0 0 25px;
		cursor: pointer;
		width: auto;

		.icon-arrow-left {
			margin-left: 4px;
			transform: rotate(90deg);
			-webkit-transform: rotate(90deg);
			-moz-transform: rotate(90deg);
			-ms-transform: rotate(90deg);
		}
	}

	h2 {
		margin-bottom: 0;
	}

	.shopBar-buy {
		text-transform: uppercase;

		&.is-disabled {
			pointer-events: none;
			opacity: .3;
		}
	}
