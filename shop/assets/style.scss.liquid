/**
 * Hero Module
 */

 .overlay {
	position: absolute;
	width: 100%;
	height: 100%;
	z-index: 99;
 }

.original-height {
	min-height: 360px;
}

 .ie .hero {min-height:800px;}
.hero {
	overflow: hidden;

	&.style-primary {
		.slide-content {
			color:  $c-text;
			&::before { background-color: $c-background; }
			.button {
				background-color: $c-primary;
				color: $c-background;
			}
		}
	}
	&.style-white {
		.slide-content {
			color: #FFFFFF;
			&::before { background-color: $c-primary; }
			.button {
				background-color: #FFFFFF;
				color: $c-primary;
			}
		}
	}
	&.style-nav-white {
		.slick-next,
		.slick-prev {
			background-color: $c-primary;
			transition: 0.3s opacity;
			&::after { color: $c-background; }
		}
	}
	&.style-nav-text {
		.slick-next,
		.slick-prev {
			background-color: #FFFFFF;
			transition: 0.3s opacity;
			&::after { color: $c-primary; }
		}
		.slick-dots {
			li {
				border-color: $c-text;
				&.slick-active { background-color: $c-text; }
			}
		}
	}

	@media screen and (min-width: $med) {
		&:hover {
			.slick-prev, .slick-next {
				/**
				 * Same as default state.
				 */
				opacity:1;
				&:hover { opacity: 1; }
			}
		}
	}
	&.hero--border {
		/**
		 * Hero--border border removed under $xs.
		 */
		@media screen and (min-width: $xs) {
			margin-top: 20px;
			margin-bottom: 20px;
			width: calc(100% - 40px);
		}
	}
	/**
	 * This remains if the slideshow is disabled too, so don't delete.
	 */
	.hero-slide {
		@include blockAb;
		bottom: 0;
		left: 0;
		right: 0;
		top: 0;
	}
	/*.slide-inner:not(.bg-loaded) .slide-content { opacity: 0; }*/
	.slide-content {
		padding: 30px 0;
		position: relative;
		.slide-content__subheading,
		.slide-content__heading,
		.slide-content__text,
		.button {
			position: relative;
			z-index: 9;
		}

		.slide-content__subheading {
			text-transform: uppercase;
			letter-spacing: 0.05em;
			font-family: $general_font;
			color: inherit;
		}

		.slide-content__heading {
			@include h0;
		}

		.slide-content__text {
			font-size: $font_bodytext_size * 1.37;
			line-height: 1.4;
			margin-bottom: 0.2em;
			p {
				font-size: inherit;
				line-height: inherit;
				font-family: inherit;
				margin: 0;
			}

			&.size--small {
				font-size: $font_bodytext_size * 1.37 * $size_scale_small;
			}

			&.size--large {
				font-size: $font_bodytext_size * 1.37 * $size_scale_large;
			}

			&.size--xlarge {
				font-size: $font_bodytext_size * 1.37 * $size_scale_xlarge;
			}
		}

		.button {
			@include transition(.3s opacity);
			margin-left: 10px;
			margin-right: 10px;
			margin-top: 15px;
			min-width: 150px;
			padding-left: 15px;
			padding-right: 15px;
			&:hover { opacity: 0.8; }

			@media screen and (max-width: $xs) {
				margin-left: 0;
				margin-right: 0;
				padding-left: 10px;
				padding-right: 10px;
				width: 100%;
			}
		}
		&::before {
			content: '';
			height: 100%;
			left: 0;
			opacity: 0.5;
			position: absolute;
			top: 0;
			width: 100%;
			z-index: 0;
		}
	}

	/**
	 * Slider Related
	 */
	.hero-dots-row {
		margin: 0 auto;
		max-width: calc(1600px * 0.85);
	}
	.slick-list {
		bottom: 0; left: 0; right: 0; top: 0;
		position: absolute;
	}

	.slick-track { line-height: 0; }
	.hero-slide { line-height: normal; }

	&.original-height {
		padding: 0;
		height: auto !important;
		min-height: 0;

		.slick-list {
			position: relative;
		}

		.slide-text {
			position: absolute;
			top: 0;
			right: 0;
			left: 0;
			bottom: 0;
			display: flex;
			align-items: center;
			justify-content: center;

			.bodyWrap > .section-slideshow:first-of-type & {
				padding-top: 60px;
			}
		}

		.slide-image {
			position: static;
			overflow: hidden;
			height: auto;
			display: flex;
			align-items: center;

			img {
				position: relative;
				display: block;
				width: 100%;
				height: auto;
				object-fit: cover;
			}
		}

		.slide-image--blank {
			width: 100%;
			height: 60vh;
			opacity: .3;

			.placeholder-svg {
				width: 100%;
				height: 100%;
			}
		}

		.hero-slide,
		.slick-list,
		.slick-track,
		.slick-slide,
		.slide-inner {
			height: auto;
		}

		.hero-slide {
			position: relative;
		}

		.slide-background-container {
			position: relative;
			height: 360px;
		}

		.slide-background {
			height: 100%;
		}
	}

	&[data-current-style="black"] {
		& + .hero-scroll-down {
			color: black;
		}

		button.slick-next::after,
		button.slick-prev::after {
			color: black;
		}

		.slick-dots li { border-color: black;}
		.slick-dots li.slick-active { background-color: black; }
	}

	&[data-current-style="white"] {
		& + .hero-scroll-down {
			color: white;
		}
		button.slick-next::after,
		button.slick-prev::after {
			color: white;
		}

		.slick-dots li { border-color: white;}
		.slick-dots li.slick-active { background-color: white; }
	}

	&:not(.slick-initialized) {
		.hero-slide:not(.initial-slide) {
			display: none;
		}
	}
}

.hero--zoom-out,
.gallery-slider--zoom-out {
	.slide-image img,
	.slide-image--blank,
	.slide-background {
		backface-visibility: hidden;
		-webkit-backface-visibility: hidden;
		will-change: transform;
		@include transform(unquote('scale(1.15)'));
		@include transition(unquote('transform 0s linear .3s, opacity 0s linear .3s !important'));
	}
	.slick-current .slide-image img.lazyloaded,
	.slick-current .slide-image--blank,
	.slick-current .slide-background.lazyloaded,
	.slick-current .slide-background--placeholder {
		@include transform(unquote('scale(1)'));
		@include transition(unquote('transform 3s ease-out, opacity .3s ease-out 0s !important'));
	}

	&.no-transition .slide-image img,
	&.no-transition .slide-background,
	&.no-transition .slick-slide {
		@include transition(unquote('none !important'));
	}
}

.hero--zoom-out.hero--is-loading,
.gallery-slider--zoom-out.gallery-slider--is-loading {
	.slide-image img,
	.slide-image--blank,
	.slide-background {
		@include transform(unquote('scale(1.15)'));
		@include transition(unquote('transform 0s linear .3s'));
	}
}

.hero--text-animation {
	.slide-slide {
		backface-visibility: hidden;
		-webkit-backface-visibility: hidden;
	}

	.slide-content__subheading,
	.slide-content__heading,
	.slide-content__text,
	.slide-content__buttons {
		opacity: 0;
		backface-visibility: hidden;
		-webkit-backface-visibility: hidden;
		will-change: opacity;
		@include transform(unquote('translate3d(0, 10px, 0)'));
		@include transition(unquote('transform .5s ease-in 0s, opacity .5s ease-in 0s'));
	}



	.slick-current {
		.slide-content__subheading,
		.slide-content__heading,
		.slide-content__text,
		.slide-content__buttons {
			opacity: 1;
			@include transform(unquote('translate3d(0, 0, 0)'));
		}

		.slide-content__subheading { @include transition(unquote('transform .5s ease-out 0.5s, opacity .5s ease-out 0.5s')); }
		.slide-content__heading { @include transition(unquote('transform .5s ease-out 0.75s, opacity .5s ease-out 0.75s')); }
		.slide-content__text { @include transition(unquote('transform .5s ease-out 1s, opacity .5s ease-out 1s')); }
		.slide-content__buttons { @include transition(unquote('transform .5s ease-out 1.25s, opacity .5s ease-out 1.25s')); }
	}

	&.hero--is-loading {
		.slide-content__subheading,
		.slide-content__heading,
		.slide-content__text,
		.slide-content__buttons {
			opacity: 0;
			@include transform(unquote('translate3d(0, 10px, 0)'));
		}
	}
}
