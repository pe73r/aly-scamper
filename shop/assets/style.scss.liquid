}
.popover-error {
	@include blockRel;

	color: {{settings.color_secondary_new}};
	padding: 10px 20px;
	text-align: left;
	width: 250px;
}
.popover-inner {
	@include blockRel;

	cursor: pointer; // clickable to open cart
	padding: 10px 20px 20px;
	text-align: left;
	width: 250px;

	&::before,
		&::after {
			content: " ";
			display: table;
		}

		&::after {
			clear: both;
		}
}
.popover-item-thumb {
	@include blockRel;
	background-size: contain;
	background-repeat: no-repeat;
	background-position: center center;
	height: 75px;
	float: left;
	margin-right: 20px;
